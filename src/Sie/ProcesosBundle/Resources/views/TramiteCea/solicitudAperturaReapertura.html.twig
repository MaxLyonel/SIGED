                                                        <div class="col-md-4">
                                                            <strong>Código Edificio Educativo:</strong>
                                                        </div>
                                                        <div class="col-md-8 form-inline">
                                                            {{ datos[tramite_tipo.tramiteTipo].lejurisdiccion }}
                                                        </div>
                                                    </div>
                                                {% endif %}
                                                <div class="col-md-6">
