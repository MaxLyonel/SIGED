                                <div class="row">
                                    <div class="col-md-12">
                                        <h4 class="page-header">
                                            DATOS GENERALES<br>
                                        </h4>
                                        <div class="form-body">
                                            <div class="row">
                                                <div class="form-group col-md-12">
                                                    <div class="col-md-4">
                                                        <strong>Nombre del CEA:</strong>
                                                    </div>
                                                    <div class="col-md-8">
                                                        {{ datos[tramite_tipo.tramiteTipo].institucioneducativa }}
                                                    </div>
                                                </div>
                                                <div class="form-group col-md-12">
                                                    <div class="col-md-4">
                                                        <strong>Fecha de Fundación:</strong>
                                                    </div>
                                                    <div class="col-md-8">
                                                        {{ datos[tramite_tipo.tramiteTipo].fechafundacion }}
                                                    </div>
                                                </div>
                                                <div class="form-group col-md-12">
                                                    <div class="col-md-4">
                                                        <strong>Teléfono de Referencia:</strong>
                                                    </div>
                                                    <div class="col-md-8">
                                                        {{ datos[tramite_tipo.tramiteTipo].telefono }}
                                                    </div>
                                                </div>
                                                <div class="form-group col-md-12">
                                                    <div class="col-md-4">
                                                        <strong>Datos del Director/Encargado:</strong>
                                                    </div>
                                                    <div class="col-md-8">
                                                        {{ datos[tramite_tipo.tramiteTipo].director }}
                                                    </div>
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </div>
                            <div class="tab-pane fade inner-all" id="tab1-2">
