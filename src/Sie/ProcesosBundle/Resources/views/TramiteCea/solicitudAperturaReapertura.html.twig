                                                        <div class="col-md-5">
                                                            <strong>Departamento:</strong>
                                                        </div>
                                                        <div class="col-md-7">
                                                            {{ datos[tramite_tipo.tramiteTipo].departamento2001 }}
                                                        </div>
                                                    </div>
                                                    <div class="form-group  col-md-12">
                                                        <div class="col-md-5">
                                                            <strong>Provincia:</strong>
                                                        </div>
                                                        <div class="col-md-7">
                                                            {{ datos[tramite_tipo.tramiteTipo].provincia2001 }}
