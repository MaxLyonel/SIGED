                                                    <table class="table table-bordered table-striped" id="tabla">
                                                        <tr>
                                                            <th>Código SIE</th>
                                                            <th>Nombre de la Institución Educativa</th>
                                                            <th>Subsistema</th>
                                                            <th>Dependencia</th>
                                                        </tr>
                                                        <tr>
                                                            <td>{{ datos[tramite_tipo.tramiteTipo].siecomparte }}</td>
                                                            <td>{{ datos[tramite_tipo.tramiteTipo].siecompartedata.nombre }}</td>
                                                            <td>{{ datos[tramite_tipo.tramiteTipo].siecompartedata.subsistema }}</td>
                                                            <td>{{ datos[tramite_tipo.tramiteTipo].siecompartedata.dependencia }}</td>
                                                        </tr>
                                                    </table>
                                                </div></br>
                                            {% endif %}
                                            <div class="row">
