                                                        <th>Administrativo</th>
                                                        <th>Docente (Maestros)</th>
                                                    </tr>
                                                    <tr>
                                                        <td>{{ datos[tramite_tipo.tramiteTipo].cantidad_adm }}</td>
                                                        <td>{{ datos[tramite_tipo.tramiteTipo].cantidad_maestro }}</td>
