                                        <div class="form-group col-md-6 form-inline">
                                            <strong>Latitud:</strong> {{ datos[tramite_tipo.tramiteTipo].latitud }}
                                            <input type="hidden" id="latitud2"
                                                   value="{{ datos[tramite_tipo.tramiteTipo].latitud }}">
                                        </div>
                                        <div class="form-group col-md-6 form-inline">
                                            <strong>Longitud:</strong> {{ datos[tramite_tipo.tramiteTipo].longitud }}
