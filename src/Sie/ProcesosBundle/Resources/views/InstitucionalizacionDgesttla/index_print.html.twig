    <script>
        function instituto(idDepartamento) {
            $.ajax({
                type: 'get',
                url: Routing.generate('institucionalizacion_buscar_instituto', {'idDepartamento': idDepartamento}),
                beforeSend: function () {
                    $("#form_instituto").empty();
                    $("#form_instituto").append('<option value="">Buscando...</option>');
                    $("#form_cargo").empty();
                    $("#form_cargo").append('<option value="">Buscando...</option>');
                },
                success: function (data) {
                    $("#form_instituto").empty();
                    $("#form_instituto").append('<option value="">Seleccionar...</option>');
                    $("#form_cargo").empty();
                    $("#form_cargo").append('<option value="">Seleccionar...</option>');
                    $.each(data.institutos, function (i, value) {
                        $("#form_instituto").append('<option value="' + i + '">' + value + '</option>');
                    });
                    
                }
            });
        }

        function cargo(idInstituto) {
            $.ajax({
                type: 'get',
                url: Routing.generate('institucionalizacion_buscar_cargo', {'idInstituto': idInstituto}),
                beforeSend: function () {
                    $("#form_cargo").empty();
                    $("#form_cargo").append('<option value="">Buscando...</option>');
                },
                success: function (data) {
                    $("#form_cargo").empty();
                    $("#form_cargo").append('<option value="">Seleccionar...</option>');
                    $.each(data.cargos, function (i, value) {
                        $("#form_cargo").append('<option value="' + i + '">' + value + '</option>');
                    });
                    
                }
            });
        }

        function validar(){
            if(!confirm("Verifique los datos del formulario antes de enviar.")){
                return false;
            }else if($('#form_carnet').val()==''){
                alert('Debe ingresar el número de Carnet de Identidad.');
                return false;
            }else if($('#form_departamento').val()<=0){
                alert('Debe ingresar el Departamento.');
                return false;
            }else if($('#form_instituto').val()<=0){
                alert('Debe ingresar el Intituto.');
                return false;
            }else if($('#form_cargo').val()<=0){
                alert('Debe ingresar el Cargo.');
                return false;
            }
        }
    </script>
