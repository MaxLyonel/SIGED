<script>
    $(function(){
            $(".validar").keydown(function(event){
                if((event.keyCode < 48 || event.keyCode > 57) && (event.keyCode < 96 || event.keyCode > 105) && event.keyCode !==190  && event.keyCode !==110 && event.keyCode !==8 && event.keyCode !==9  ){
                    return false;
                }
            });
        });
    $(".alquiler").change(function () {	 
        if ($(this).val() == 'SI'){
            $('#contrato').show();
            $('#form_i_contrato_ampliar').attr('required',true);

        }else{
            $('#contrato').hide();
            $('#form_i_contrato_ampliar').attr('required',false);
            $('#form_i_contrato_ampliar').val('');
        }
    });
    $(".alquilerDependencia").change(function () {	 
        if ($(this).val() == 'SI'){
            $('#contratoDependencia').show();
            $('#form_i_contrato_dependencia').attr('required',true);

        }else{
            $('#contratoDependencia').hide();
            $('#form_i_contrato_dependencia').attr('required',false);
            $('#form_i_contrato_dependencia').val('');
        }
    });
    $(".alquilerReducir").change(function () {	 
        if ($(this).val() == 'SI'){
            $('#contratoReducir').show();
            $('#form_i_contrato_reducir').attr('required',true);

        }else{
            $('#contratoReducir').hide();
            $('#form_i_contrato_reducir').attr('required',false);
            $('#form_i_contrato_reducir').val('');
        }
    });
    $("#form_i_form_fundaempresa").change(function () {	 
        if ($(this).is(':checked')){
            $('#fundaempresaDependencia').show();
        }else{
            $('#fundaempresaDependencia').hide();
        }
    });
    $("#form_i_fotocopia_nit").change(function () {	 
        if ($(this).is(':checked')){
            $('#nitDependencia').show();
        }else{
            $('#nitDependencia').hide();
        }
    });
    $("#form_i_copianit_cn").change(function () {	 
        if ($(this).is(':checked')){
            $('#nitCn').show();
        }else{
            $('#nitCn').hide();
        }
    });
    $("#form_i_form_fundaempresa_apertura").change(function () {
        $('#form_nro_fundaempresa_apertura').val('');
        $('#form_fecha_fundaempresa_apertura').val('');
        if ($(this).is(':checked')){
            $('#fundaempresaApertura').show();
        }else{
            $('#fundaempresaApertura').hide();
        }
    });
    $("#form_i_fotocopia_nit_apertura").change(function () {
        $('#form_nit_apertura').val('');
        $('#form_i_balance_apertura').attr('checked',false);	 
        if ($(this).is(':checked')){
            $('#nitApertura').show();
        }else{
            $('#nitApertura').hide();
        }
    });
    $(".alquilerApertura").change(function () {	 
        $('#form_ii_contrato_apertura').val('');
        $('#form_ii_folio_apertura').attr('checked',false);
        if ($(this).val() == 'SI'){
            $('#contratoApertura').show();
            $('#folioApertura').hide();
        }else{
            $('#contratoApertura').hide();
            $('#folioApertura').show();
        }
    });
    $(".req34").click(function(){
        $('#validar34').val(0);
        $('#msg34').empty();
    });
    $(".req35").click(function(){
        $('#validar35').val(0);
        $('#msg35').empty();
    });
    $(".req36").click(function(){
        $('#validar36').val(0);
        $('#msg36').empty();
    });
    $(".req37").click(function(){
        $('#validar37').val(0);
        $('#msg37').empty();
    });
    $(".req38").click(function(){
        $('#validar38').val(0);
        $('#msg38').empty();
    });
    $(".req41").click(function(){
        $('#validar41').val(0);
        $('#msg41').empty();
    });
    $(".req42").click(function(){
        if($('#validar42').length == 1){
            $('#validar42').val(0);
            $('#msg42').empty();    
        }else{
            $('#validar43').val(0);
            $('#msg43').empty();
        }
    });
    $(".req45").click(function(){
        $('#validar45').val(0);
        $('#msg45').empty();
    });
    $(".req54").click(function(){
        $('#validar54').val(0);
        $('#msg54').empty();
    });  
</script>