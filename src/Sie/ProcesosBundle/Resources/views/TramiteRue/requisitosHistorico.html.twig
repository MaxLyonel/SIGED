{% for t in datos.tramites  %}
    {% if t.id == 34 %} {# ampliacion de nivel #}
        <div class="modal fade borrar"  role="dialog" aria-hidden="true" id="modal_{{t.id}}">
            <div class="modal-dialog modal-lg" id="modal_{{t.id}}">
                <div class="modal-content">
                    <div class="panel panel-primary">
                        <div class="panel-heading">
                            <div class="pull-left">
                                <h3 class="panel-title"><span>REQUISITOS PRESENTADOS: {{t.tramite_tipo|upper}}</span></h3>
                            </div>
                            <div class="pull-rigth">
                                <button type="button" class="close" data-dismiss="modal" aria-hidden="true">x</button>
                            </div>
                            <div class="clearfix"></div>
                        </div>
                        <div class="panel-body">
                            <div class="row">
                                <div class="col-md-12">
                                    <!-- Start basic wizard horizontal -->
                                    <div id="basic-wizard-horizontal">
                                        <div class="panel panel-tab rounded shadow">
                                            <!-- Start tabs heading -->
                                            <div class="panel-heading no-padding">
                                                <ul class="nav nav-tabs nav-pills">
                                                    <li class="active">
                                                        <a href="#tab1-1-{{t.id}}" data-toggle="tab" aria-expanded="false">
                                                            <i class="fa fa-file-text"></i>
                                                            <div>
                                                                <span class="text-strong">1. Requisitos Legales</span>
                                                            </div>
                                                        </a>
                                                    </li>
                                                    <li class="">
                                                        <a href="#tab1-2-{{t.id}}" data-toggle="tab" aria-expanded="false">
                                                            <i class="glyphicon glyphicon-home"></i>
                                                            <div>
                                                                <span class="text-strong">2. Requisitos de infraestructura</span>
                                                            </div>
                                                        </a>
                                                    </li>
                                                </ul>
                                            </div><!-- /.panel-heading -->
                                            <!--/ End tabs heading -->
                                            <!-- Start tabs content -->
                                            <div class="panel-body tab-content">
                                                <div class="tab-pane fade inner-all in active" id="tab1-1-{{t.id}}">
                                                    <div class="row">
                                                        <div  class="form-group col-md-12">
                                                            <div class="col-md-6">
                                                                Solicitud de Ampliación de Nivel:
                                                            </div>
                                                            <div class="col-md-6">
                                                                <a  class="btn btn-lilac btn-xs" href="{{asset('uploads/archivos/flujos/'~tramite.institucioneducativa.id~'/rue/'~tramite.gestionId~'/'~ datos[t.tramite_tipo].i_solicitud_ampliar )}}" target="_blank" data-placement="top" title="Ver informe">
                                                                    <i class="fa fa-file-pdf-o fa-2x" >  </i>
                                                                </a>
                                                            </div>
                                                        </div>
                                                        <div class="form-group col-md-12">
                                                            <div class="col-md-6">
                                                                Infraestructura arrendada:
                                                            </div>
                                                            <div class="col-md-6">
                                                                {{datos[t.tramite_tipo].i_alquiler_ampliar}}
                                                            </div>
                                                        </div>
                                                        {% if datos[t.tramite_tipo].i_alquiler_ampliar == 'SI' %}
                                                            <div  class="form-group col-md-12">
                                                                <div class="col-md-6">
                                                                    Copia notariada de arrendamiento:
                                                                </div>
                                                                <div class="col-md-6">
                                                                    <a  class="btn btn-lilac btn-xs" href="{{asset('uploads/archivos/flujos/'~tramite.institucioneducativa.id~'/rue/'~tramite.gestionId~'/'~ datos[t.tramite_tipo].i_contrato_ampliar )}}" target="_blank" data-placement="top" title="Ver informe">
                                                                        <i class="fa fa-file-pdf-o fa-2x" >  </i>
                                                                    </a>
                                                                </div>
                                                            </div>
                                                        {% endif %}
                                                        <div  class="form-group col-md-12 ">
                                                            <div class="ckbox ckbox-primary">
                                                                <input id="checkbox-primary" checked="checked" type="checkbox" disabled="disabled">
                                                                <label for="checkbox-primary">Original de Certificado RUE</label>
                                                            </div>
                                                        </div>
                                                    </div>
                                                </div>
                                                <div class="tab-pane fade inner-all" id="tab1-2-{{t.id}}">
                                                    <div class="row">
                                                        <div class="ckbox ckbox-primary">
                                                            <input id="checkbox-primary1" checked="checked" type="checkbox" disabled="disabled">
                                                            <label for="checkbox-primary1">Planos arquitectónicos</label>
                                                        </div>
                                                        <div class="ckbox ckbox-primary">
                                                            <input id="checkbox-primary1" checked="checked" type="checkbox" disabled="disabled">
                                                            <label for="checkbox-primary1">Actualización de datos de infraestructura en el SIE</label>
                                                        </div>
                                                    </div>
                                                </div>
                                            </div><!-- /.panel-body -->
                                            <!--/ End tabs content -->
                                            <!--/ End pager -->
                                        </div><!-- /.panel -->
                                    </div><!-- /#basic-wizard-horizontal -->
                                    <!--/ End basic wizard horizontal-->
                                </div>
                            </div>
                        </div>
                        <div class="panel-footer">
                            <div class="pull-right">
                                <button type="button" class="btn btn-danger" data-dismiss="modal">Cerrar</button>
                            </div>
                            <div class="clearfix"></div>
                        </div>
                    </div>
                </div>
            </div><!-- /.modal-dialog -->
        </div>
    {% elseif t.id == 35 %}{# Reduccion de Nivel #}
        <div class="modal fade borrar"  role="dialog" aria-hidden="true" id="modal_{{t.id}}">
            <div class="modal-dialog modal-lg" id="modal_{{t.id}}">
                <div class="modal-content">
                    <div class="panel panel-primary">
                        <div class="panel-heading">
                            <div class="pull-left">
                                <h3 class="panel-title"><span>REQUISITOS PRESENTADOS: {{t.tramite_tipo|upper}}</span></h3>
                            </div>
                            <div class="pull-rigth">
                                <button type="button" class="close" data-dismiss="modal" aria-hidden="true">x</button>
                            </div>
                            <div class="clearfix"></div>
                        </div>
                        <div class="panel-body">
                            <div class="row">
                                <div class="col-md-12">
                                    <!-- Start basic wizard horizontal -->
                                    <div id="basic-wizard-horizontal">
                                        <div class="panel panel-tab rounded shadow">
                                            <!-- Start tabs heading -->
                                            <div class="panel-heading no-padding">
                                                <ul class="nav nav-tabs nav-pills">
                                                    <li class="active">
                                                        <a href="#tab1-1-{{t.id}}" data-toggle="tab" aria-expanded="false">
                                                            <i class="fa fa-file-text"></i>
                                                            <div>
                                                                <span class="text-strong">1. Requisitos Legales</span>
                                                            </div>
                                                        </a>
                                                    </li>
                                                    <li class="">
                                                        <a href="#tab1-2-{{t.id}}" data-toggle="tab" aria-expanded="false">
                                                            <i class="glyphicon glyphicon-home"></i>
                                                            <div>
                                                                <span class="text-strong">2. Requisitos de infraestructura</span>
                                                            </div>
                                                        </a>
                                                    </li>
                                                </ul>
                                            </div><!-- /.panel-heading -->
                                            <!--/ End tabs heading -->
                                            <!-- Start tabs content -->
                                            <div class="panel-body tab-content">
                                                <div class="tab-pane fade inner-all in active" id="tab1-1-{{t.id}}">
                                                    <div class="row">
                                                        <div  class="form-group col-md-12">
                                                            <div class="col-md-8">
                                                                Solicitud de Reducción de Nivel:
                                                            </div>
                                                            <div class="col-md-4">
                                                                <a  class="btn btn-lilac btn-xs" href="{{asset('uploads/archivos/flujos/'~tramite.institucioneducativa.id~'/rue/'~tramite.gestionId~'/'~ datos[t.tramite_tipo].i_solicitud_reducir )}}" target="_blank" data-placement="top" title="Ver informe">
                                                                    <i class="fa fa-file-pdf-o fa-2x" >  </i>
                                                                </a>
                                                            </div>
                                                        </div>
                                                        <div class="form-group col-md-12">
                                                            <div class="col-md-8">
                                                                Infraestructura arrendada:
                                                            </div>
                                                            <div class="col-md-4">
                                                                {{datos[t.tramite_tipo].i_alquiler_reducir}}
                                                            </div>
                                                        </div>
                                                        {% if datos[t.tramite_tipo].i_alquiler_reducir == 'SI' %}
                                                            <div  class="form-group col-md-12">
                                                                <div class="col-md-8">
                                                                    Copia notariada de arrendamiento:
                                                                </div>
                                                                <div class="col-md-4">
                                                                    <a  class="btn btn-lilac btn-xs" href="{{asset('uploads/archivos/flujos/'~tramite.institucioneducativa.id~'/rue/'~tramite.gestionId~'/'~ datos[t.tramite_tipo].i_contrato_reducir )}}" target="_blank" data-placement="top" title="Ver informe">
                                                                        <i class="fa fa-file-pdf-o fa-2x" >  </i>
                                                                    </a>
                                                                </div>
                                                            </div>
                                                        {% endif %}
                                                        <div  class="form-group col-md-12 ">
                                                            <div class="ckbox ckbox-primary">
                                                                <input id="checkbox-primary" checked="checked" type="checkbox" disabled="disabled">
                                                                <label for="checkbox-primary">Original de Certificado RUE</label>
                                                            </div>
                                                        </div>
                                                    </div>
                                                </div>
                                                <div class="tab-pane fade inner-all" id="tab1-2-{{t.id}}">
                                                    <div class="row">
                                                        <div class="ckbox ckbox-primary">
                                                            <input id="checkbox-primary1" checked="checked" type="checkbox" disabled="disabled">
                                                            <label for="checkbox-primary1">Planos arquitectónicos</label>
                                                        </div>
                                                        <div class="ckbox ckbox-primary">
                                                            <input id="checkbox-primary1" checked="checked" type="checkbox" disabled="disabled">
                                                            <label for="checkbox-primary1">Actualización de datos de infraestructura en el SIE</label>
                                                        </div>
                                                    </div>
                                                </div>
                                            </div><!-- /.panel-body -->
                                            <!--/ End tabs content -->
                                            <!--/ End pager -->
                                        </div><!-- /.panel -->
                                    </div><!-- /#basic-wizard-horizontal -->
                                    <!--/ End basic wizard horizontal-->
                                </div>
                            </div>
                        </div>
                        <div class="panel-footer">
                            <div class="pull-right">
                                <button type="button" class="btn btn-danger" data-dismiss="modal">Cerrar</button>
                            </div>
                            <div class="clearfix"></div>
                        </div>
                    </div>
                </div>
            </div><!-- /.modal-dialog -->
        </div>
    {% elseif t.id == 36 %}{# cambio de dependencia #}
        <div class="modal fade borrar"  role="dialog" aria-hidden="true" id="modal_{{t.id}}">
            <div class="modal-dialog modal-lg" id="modal_{{t.id}}">
                <div class="modal-content">
                    <div class="panel panel-primary">
                        <div class="panel-heading">
                            <div class="pull-left">
                                <h3 class="panel-title"><span>REQUISITOS PRESENTADOS: {{t.tramite_tipo|upper}}</span></h3>
                            </div>
                            <div class="pull-rigth">
                                <button type="button" class="close" data-dismiss="modal" aria-hidden="true">x</button>
                            </div>
                            <div class="clearfix"></div>
                        </div>
                        <div class="panel-body">
                            <div class="row">
                                <div class="col-md-12">
                                    <!-- Start basic wizard horizontal -->
                                    <div id="basic-wizard-horizontal">
                                        <div class="panel panel-tab rounded shadow">
                                            <!-- Start tabs heading -->
                                            <div class="panel-heading no-padding">
                                                <ul class="nav nav-tabs nav-pills">
                                                    <li class="active">
                                                        <a href="#tab1-1-{{t.id}}" data-toggle="tab" aria-expanded="false">
                                                            <i class="fa fa-file-text"></i>
                                                            <div>
                                                                <span class="text-strong">1. Requisitos Legales</span>
                                                            </div>
                                                        </a>
                                                    </li>
                                                </ul>
                                            </div><!-- /.panel-heading -->
                                            <!--/ End tabs heading -->
                                            <!-- Start tabs content -->
                                            <div class="panel-body tab-content">
                                                <div class="tab-pane fade inner-all in active" id="tab1-1-{{t.id}}">
                                                    <div class="row">
                                                        <div  class="form-group col-md-12">
                                                            {% if datos.institucioneducativa.dependencia_tipo_id == 1 %}
                                                                <div class="col-md-8">
                                                                    Solicitud de Cambio de dependencia:
                                                                </div>
                                                                <div class="col-md-4">
                                                                    <a  class="btn btn-lilac btn-xs" href="{{asset('uploads/archivos/flujos/'~tramite.institucioneducativa.id~'/rue/'~tramite.gestionId~'/'~ datos[t.tramite_tipo].i_solicitud_dependencia )}}" target="_blank" data-placement="top" title="Ver solicitud">
                                                                        <i class="fa fa-file-pdf-o fa-2x" >  </i>
                                                                    </a>
                                                                </div>
                                                            {% else %}
                                                                <div class="col-md-8">
                                                                    Informe estableciendo aspectos técnicos de infraestructura y de cambio de dependencia:
                                                                </div>
                                                                <div class="col-md-4">
                                                                    <a  class="btn btn-lilac btn-xs" href="{{asset('uploads/archivos/flujos/'~tramite.institucioneducativa.id~'/rue/'~tramite.gestionId~'/'~ datos[t.tramite_tipo].i_informe_dependencia )}}" target="_blank" data-placement="top" title="Ver informe">
                                                                        <i class="fa fa-file-pdf-o fa-2x" >  </i>
                                                                    </a>
                                                                </div>
                                                            {% endif %}
                                                        </div>
                                                        {% if datos.institucioneducativa.dependencia_tipo_id == 1 %}
                                                            <div class="form-group col-md-12">
                                                                <div class="col-md-8">
                                                                    Acta de Fundación:
                                                                </div>
                                                                <div class="col-md-4">
                                                                    <a  class="btn btn-lilac btn-xs" href="{{asset('uploads/archivos/flujos/'~tramite.institucioneducativa.id~'/rue/'~tramite.gestionId~'/'~ datos[t.tramite_tipo].i_actafundacion )}}" target="_blank" data-placement="top" title="Ver informe">
                                                                        <i class="fa fa-file-pdf-o fa-2x" >  </i>
                                                                    </a>
                                                                </div>
                                                            </div>
                                                            <div class="form-group col-md-12">
                                                                <div class="ckbox ckbox-primary">
                                                                    <input id="checkbox-primary" checked="checked" type="checkbox" disabled="disabled">
                                                                    <label for="checkbox-primary">Folio Real emitido po Derechos Reales a nombre del Gobierno Autónomo Municipal correspondiente.</label>
                                                                </div>
                                                            </div>
                                                            <div class="form-group col-md-12">
                                                                <div class="col-md-8">
                                                                    Nro. de Folio Real:
                                                                </div>
                                                                <div class="col-md-4">
                                                                    {{ datos[t.tramite_tipo].i_nrofolio_dependencia }}
                                                                </div>
                                                            </div>
                                                            <div class="form-group col-md-12">
                                                                <div class="col-md-8">
                                                                    Infraestructura arrendada:
                                                                </div>
                                                                <div class="col-md-4">
                                                                    {{datos[t.tramite_tipo].i_esalquiler_dependencia}}
                                                                </div>
                                                            </div>
                                                            {% if datos[t.tramite_tipo].i_esalquiler_dependencia == 'SI' %}
                                                                <div class="form-group col-md-12">
                                                                    <div class="col-md-8">
                                                                        Copia notariada de contrato de arrendamiento:
                                                                    </div>
                                                                    <div class="col-md-4">
                                                                        <a  class="btn btn-lilac btn-xs" href="{{asset('uploads/archivos/flujos/'~tramite.institucioneducativa.id~'/rue/'~tramite.gestionId~'/'~ datos[t.tramite_tipo].i_contrato_dependencia )}}" target="_blank" data-placement="top" title="Ver informe">
                                                                            <i class="fa fa-file-pdf-o fa-2x" >  </i>
                                                                        </a>
                                                                    </div>
                                                                </div>
                                                            {% endif %}
                                                            <div class="form-group col-md-12">
                                                                <div class="col-md-8">
                                                                    Convenio vigente de prestación de servicios:
                                                                </div>
                                                                <div class="col-md-4">
                                                                    <a  class="btn btn-lilac btn-xs" href="{{asset('uploads/archivos/flujos/'~tramite.institucioneducativa.id~'/rue/'~tramite.gestionId~'/'~ datos[t.tramite_tipo].i_convenio_dependencia )}}" target="_blank" data-placement="top" title="Ver informe">
                                                                        <i class="fa fa-file-pdf-o fa-2x" >  </i>
                                                                    </a>
                                                                </div>
                                                            </div>
                                                            <div class="form-group col-md-12">
                                                                <div class="col-md-8">
                                                                    Área geográfica establecida por el municipio:
                                                                </div>
                                                                <div class="col-md-4">
                                                                    {{datos[t.tramite_tipo].i_area_dependencia}}
                                                                </div>
                                                            </div>
                                                            <div class="form-group col-md-12">
                                                                <div class="col-md-8">
                                                                    Certificación emitida por el Gobierno Autónomo Municipal correspondiente, estableciendo si la unidad educativa es del área rural o urbana:
                                                                </div>
                                                                <div class="col-md-4">
                                                                    <a  class="btn btn-lilac btn-xs" href="{{asset('uploads/archivos/flujos/'~tramite.institucioneducativa.id~'/rue/'~tramite.gestionId~'/'~ datos[t.tramite_tipo].i_certificacion_gam )}}" target="_blank" data-placement="top" title="Ver informe">
                                                                        <i class="fa fa-file-pdf-o fa-2x" >  </i>
                                                                    </a>
                                                                </div>
                                                            </div>
                                                            <div class="form-group col-md-12">
                                                                <div class="col-md-8">
                                                                    Certificación de convenio emitida por el Responsable de Institución Prestadora de Servicios:
                                                                </div>
                                                                <div class="col-md-4">
                                                                    <a  class="btn btn-lilac btn-xs" href="{{asset('uploads/archivos/flujos/'~tramite.institucioneducativa.id~'/rue/'~tramite.gestionId~'/'~ datos[t.tramite_tipo].i_certificacionde_convenio )}}" target="_blank" data-placement="top" title="Ver informe">
                                                                        <i class="fa fa-file-pdf-o fa-2x" >  </i>
                                                                    </a>
                                                                </div>
                                                            </div>
                                                            <div class="form-group col-md-12">
                                                                <div class="ckbox ckbox-primary">
                                                                    <input id="checkbox-primary" {% if datos[t.tramite_tipo].i_convenio_administracion == 1 %} checked="checked" {% endif %} type="checkbox" disabled="disabled">
                                                                    <label for="checkbox-primary">Convenio de administración de infraestructura firmado entre el Gobierno Autónomo Municipal correspondiente y la Institución prestadora del servicio (si corresponde).</label>
                                                                </div>
                                                            </div>
                                                            <div class="form-group col-md-12">
                                                                <div class="ckbox ckbox-primary">
                                                                    <input id="checkbox-primary" checked="checked" type="checkbox" disabled="disabled">
                                                                    <label for="checkbox-primary">Copia Notariada del Acta de Constitución y estatutos de la Institución prestadora de servicios.</label>
                                                                </div>
                                                            </div>
                                                            <div class="form-group col-md-12">
                                                                <div class="ckbox ckbox-primary">
                                                                    <input id="checkbox-primary" {% if datos[t.tramite_tipo].i_registro_culto == 1 %} checked="checked" {% endif %} type="checkbox" disabled="disabled">
                                                                    <label for="checkbox-primary">En caso de Institucionaes Religiosas, fotocopia legalizada del certificado de registro de culto emitido por el Ministerio de Relaciones Exteriores.</label>
                                                                </div>
                                                            </div>
                                                            <div class="form-group col-md-12">
                                                                <div class="ckbox ckbox-primary">
                                                                    <input id="checkbox-primary" {% if datos[t.tramite_tipo].i_org_nogubernamental == 1 %} checked="checked" {% endif %}  type="checkbox" disabled="disabled">
                                                                    <label for="checkbox-primary">Para instituciones no gubernamentales, fotocopia legalizada del certificado de registro emitido por el Viceministerio de Inversion Pública y Financiamiento Externo.</label>
                                                                </div>
                                                            </div>
                                                            <div class="form-group col-md-12">
                                                                <div class="ckbox ckbox-primary">
                                                                    <input id="checkbox-primary" {% if datos[t.tramite_tipo].i_form_fundaempresa == 1 %} checked="checked" {% endif %} type="checkbox" disabled="disabled">
                                                                    <label for="checkbox-primary">Copia legalizada del formulario de registro de comercio, emitido por FUNDAEMPRESA, (si corresponde).</label>
                                                                </div>
                                                            </div>
                                                            {% if datos[t.tramite_tipo].i_form_fundaempresa == 1 %}
                                                                <div class="form-group col-md-12">
                                                                    <div class="col-md-8">
                                                                        Nro. de Matrícula de Comercio:
                                                                    </div>
                                                                    <div class="col-md-4">
                                                                        {{datos[t.tramite_tipo].nro_fundaempresa}}
                                                                    </div>
                                                                </div>
                                                                <div class="form-group col-md-12">
                                                                    <div class="col-md-8">
                                                                        Fecha de Inscripción:
                                                                    </div>
                                                                    <div class="col-md-4">
                                                                        {{datos[t.tramite_tipo].fecha_fundaempresa}}
                                                                    </div>
                                                                </div>
                                                            {% endif %}
                                                            <div class="form-group col-md-12">
                                                                <div class="ckbox ckbox-primary">
                                                                    <input id="checkbox-primary" {% if datos[t.tramite_tipo].i_fotocopia_nit == 1 %} checked="checked" {% endif %} type="checkbox" disabled="disabled">
                                                                    <label for="checkbox-primary">Fotocopia legalizada del certificado del Número de Identificación Tributaria NIT, (si corresponde).</label>
                                                                </div>
                                                            </div>
                                                            {% if datos[t.tramite_tipo].i_fotocopia_nit == 1 %}
                                                                <div class="form-group col-md-12">
                                                                    <div class="col-md-8">
                                                                        Nro. de N.I.T.
                                                                    </div>
                                                                    <div class="col-md-4">
                                                                        {{datos[t.tramite_tipo].nit_dependencia}}
                                                                    </div>
                                                                </div>
                                                                <div class="form-group col-md-12">
                                                                    <div class="ckbox ckbox-primary">
                                                                        <input id="checkbox-primary" checked="checked" type="checkbox" disabled="disabled">
                                                                        <label for="checkbox-primary">Balance de apertura de la unidad educativa, sellado por el Servicio de Impuestos Nacionales SIN (si corresponde).</label>
                                                                    </div>
                                                                </div>
                                                            {% endif %}
                                                            <div class="form-group col-md-12">
                                                                <div class="col-md-8">
                                                                    Nro. del Testimonio de personeria jurídica de la entidad prestadora de servicio:
                                                                </div>
                                                                <div class="col-md-4">
                                                                    {{datos[t.tramite_tipo].i_testimonioconvenio}}
                                                                </div>
                                                            </div>
                                                            <div class="form-group col-md-12">
                                                                <div class="col-md-8">
                                                                    Fecha del testimonio:
                                                                </div>
                                                                <div class="col-md-4">
                                                                    {{datos[t.tramite_tipo].fecha_testimonioconvenio}}
                                                                </div>
                                                            </div>
                                                        {% endif %}
                                                        <div  class="form-group col-md-12 ">
                                                            <div class="ckbox ckbox-primary">
