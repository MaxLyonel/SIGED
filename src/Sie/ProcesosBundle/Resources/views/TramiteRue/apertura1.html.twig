<script>
    $(function(){
        $(".validar").keydown(function(event){
            if((event.keyCode < 48 || event.keyCode > 57) && (event.keyCode < 96 || event.keyCode > 105) && event.keyCode !==190  && event.keyCode !==110 && event.keyCode !==8 && event.keyCode !==9  ){
                return false;
            }
        });
    });
     $(".nivel").change(function () {	 
        if($(this).val()==11){
            $('.nivel11').val('');
            if($(this).is(':checked')==true){
                $('.nivel11').attr('disabled',false);
            }else{
                $('.nivel11').attr('disabled',true);
            }
        }
        if($(this).val()==12){
            $('.nivel12').val('');
            if($(this).is(':checked')==true){
                $('.nivel12').attr('disabled',false);
            }else{
                $('.nivel12').attr('disabled',true);
            }
        }
        if($(this).val()==13){
            $('.nivel13').val('');
            if($(this).is(':checked')==true){
                $('.nivel13').attr('disabled',false);
            }else{
                $('.nivel13').attr('disabled',true);
            }
        }
    });
    $(".req54").click(function(){
        $('#validar54').val(0);
        $('#msg54').empty();
    });
    $("#form_dependencia").change(function(){
        //alert($(this).val());
        $('#convenio').hide();
        $('#convenio').hide();
        if($(this).val()==2){
            $('#convenio').show();
        }
        if($(this).val()==3){
            $('#constitucion').show();
        }
    });
</script>