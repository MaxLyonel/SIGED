                                                            {{ form_label(form_35.i_alquiler_reducir) }}
                                                        </div>
                                                        <div class="col-md-6">
                                                            {% for a in form_35.i_alquiler_reducir %}
                                                                <div class="col-md-3 rdio rdio-primary circle">
                                                                    {{ form_widget(a) }}
                                                                    {{ form_label(a) }}
                                                                </div>
                                                            {% endfor %}
                                                        </div>
                                                    </div>
                                                    <div class="form-group col-md-12">
                                                        <div class="col-md-6">
                                                            {{ form_label(form_35.i_contrato_reducir) }}
                                                        </div>
                                                        <div class="col-md-6">
                                                            {{ form_widget(form_35.i_contrato_reducir) }}
                                                        </div>
                                                    </div>
                                                    <div class="form-group col-md-12">
                                                        <div class="col-md-6">
                                                        </div>
                                                        <div class="col-md-6 ckbox ckbox-primary">
                                                            {{ form_widget(form_35.i_certificado_reducir) }}
                                                            {{ form_label(form_35.i_certificado_reducir) }}                                                            
                                                        </div>
                                                    </div>
                                                </div>
                                            </div>
                                            <div class="tab-pane fade inner-all" id="tab1-2-{{id}}">
                                                <div class="row">
                                                    <div class="form-group col-md-12 ckbox ckbox-primary">
                                                        <div class="col-md-6">
                                                            {{ form_widget(form_35.ii_planos_reducir) }}
                                                            {{ form_label(form_35.ii_planos_reducir) }}
                                                        </div>
                                                        <div class="col-md-6">
                                                        </div>
                                                    </div>
                                                    <div class="form-group col-md-12 ckbox ckbox-primary">
                                                        <div class="col-md-6">
                                                            {{ form_widget(form_35.ii_infra_reducir) }}
                                                            {{ form_label(form_35.ii_infra_reducir) }}
                                                        </div>
                                                        <div class="col-md-6">                                                            
                                                        </div>
                                                    </div>
                                                </div>
                                            </div>
                                        {{ form_end(form_35) }}
                                    {% elseif id == 36 %}{# cambio de dependencia #}
                                        <div class="tab-pane fade inner-all in active" id="tab1-1">
                                        </div>
                                        <div class="tab-pane fade inner-all" id="tab1-2">
                                        </div>
                                        <div class="tab-pane fade inner-all" id="tab1-3">
                                        </div>                                                
                                    {% elseif id == 37 %}{# cambio de nombre #}
                                        <div class="tab-pane fade inner-all in active" id="tab1-1">
                                        </div>
                                        <div class="tab-pane fade inner-all" id="tab1-2">
                                        </div>
                                        <div class="tab-pane fade inner-all" id="tab1-3">
                                        </div>                                                
                                    {% elseif id == 38 %}{# cambio de jurisdiccion administrativa #}
                                        <div class="tab-pane fade inner-all in active" id="tab1-1">
                                        </div>
                                        <div class="tab-pane fade inner-all" id="tab1-2">
                                        </div>
                                        <div class="tab-pane fade inner-all" id="tab1-3">
                                        </div>                                                    
                                    {% elseif id == 39 %}{# Fusion #}
                                        <div class="tab-pane fade inner-all in active" id="tab1-1">
                                        </div>
                                        <div class="tab-pane fade inner-all" id="tab1-2">
                                        </div>
                                        <div class="tab-pane fade inner-all" id="tab1-3">
                                        </div>                                                    
                                    {% elseif id == 40 %} {# desglose #}
                                        <div class="tab-pane fade inner-all in active" id="tab1-1">
                                        </div>
                                        <div class="tab-pane fade inner-all" id="tab1-2">
                                        </div>
                                        <div class="tab-pane fade inner-all" id="tab1-3">
                                        </div>
                                    {% elseif id == 41 %} {# cambio de infraestructura #}
                                        <div class="tab-pane fade inner-all in active" id="tab1-1">
                                        </div>
                                        <div class="tab-pane fade inner-all" id="tab1-2">
                                        </div>
                                        <div class="tab-pane fade inner-all" id="tab1-3">
                                        </div>
                                    {% elseif id == 42 %} {# cierre temporal #}
                                        <div class="tab-pane fade inner-all in active" id="tab1-1">
                                        </div>
                                        <div class="tab-pane fade inner-all" id="tab1-2">
                                        </div>
                                        <div class="tab-pane fade inner-all" id="tab1-3">
                                        </div>
                                    {% elseif id == 43 %} {# cierre definitivo #}
                                        <div class="tab-pane fade inner-all in active" id="tab1-1">
                                        </div>
                                        <div class="tab-pane fade inner-all" id="tab1-2">
                                        </div>
                                        <div class="tab-pane fade inner-all" id="tab1-3">
                                        </div>
                                    {% elseif id == 44 %}{# Reapertura #}
                                        <div class="tab-pane fade inner-all in active" id="tab1-1">
                                        </div>
                                        <div class="tab-pane fade inner-all" id="tab1-2">
                                        </div>
                                        <div class="tab-pane fade inner-all" id="tab1-3">
                                        </div>
                                    {% elseif id == 45 %}{# nuevo certifcado rue #}
                                        <div class="tab-pane fade inner-all in active" id="tab1-1">
                                        </div>
                                        <div class="tab-pane fade inner-all" id="tab1-2">
                                        </div>
                                        <div class="tab-pane fade inner-all" id="tab1-3">
                                        </div>
                                    {% endif %}
                                </div><!-- /.panel-body -->
                                <!--/ End tabs content -->
                                <!--/ End pager -->
                            </div><!-- /.panel -->
                        </div><!-- /#basic-wizard-horizontal -->
                        <!--/ End basic wizard horizontal-->
                    </div>
                </div>
            </div>
            <div class="panel-footer">
                <div class="pull-right">
                    <button type="button" class="btn btn-primary" onclick="guardarRequisitos({{id}})">aceptar</button>
                    <button type="button" class="btn btn-danger" data-dismiss="modal">Cerrar</button>
                </div>
                <div class="clearfix"></div>
            </div>
        </div>
    </div>
</div><!-- /.modal-dialog -->

