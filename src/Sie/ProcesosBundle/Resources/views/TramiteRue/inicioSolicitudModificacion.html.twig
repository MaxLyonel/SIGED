        function comunidad(p) {
            $.ajax({
                type: 'get',
                url: Routing.generate('tramite_rue_le_buscar_comunidad', {'idMunicipio': p}),
                beforeSend: function () {
                    $("#form_comunidad").empty();
                    $("#form_comunidad").append('<option value="">Buscando...</option>');
                },
                success: function (data) {
                    $("#form_comunidad").empty();
                    $("#form_comunidad").append('<option value="">Seleccionar...</option>');
                    $.each(data.comunidad, function (i, value) {
                        $("#form_comunidad").append('<option value="' + i + '">' + value + '</option>');
                    });
                }
            });
        }
        function canton(m) {
            $.ajax({
                type: 'get',
                url: Routing.generate('tramite_rue_le_buscar_cantones', {'idMunicipio': m}),
                beforeSend: function () {
                    $("#form_canton").empty();
                    $("#form_localidad").empty();
                    $("#form_canton").append('<option value="">Buscando...</option>');
                    $("#form_localidad").append('<option value="">Buscando...</option>');
                },
                success: function (data) {
                    $("#form_canton").empty();
                    $("#form_localidad").empty();
                    $("#form_canton").append('<option value="">Seleccionar...</option>');
                    $("#form_localidad").append('<option value="">Seleccionar...</option>');
                    $.each(data.canton, function (i, value) {
                        $("#form_canton").append('<option value="' + i + '">' + value + '</option>');
                    });

