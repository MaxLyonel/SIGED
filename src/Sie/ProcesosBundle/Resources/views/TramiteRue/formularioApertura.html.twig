        <script>
            $('#sie_appwebbundle_usuario_save').click(function () {
                if ($('#sie_appwebbundle_usuario_username').val() == '' && $('#sie_appwebbundle_usuario_password').val() == '') {
                    $('#myModal').modal('show');
                    return false;
                }
                $('#sie_appwebbundle_usuario').submit();
            });
            $("#form_captcha").keyup(function () {
                $("#form_captcha").val($("#form_captcha").val().toUpperCase());
            });

            function showCaptcha() {
                element = document.getElementById("divCaptcha");
                check = document.getElementById("form_check");
                captcha = document.getElementById("form_captcha");
                if (check.checked) {
                    element.style.display = 'none';
                    $('#form_captcha').removeAttr("required");
                    $('#form_captcha').val('');
                }
                else {
                    element.style.display = 'block';
                    $('#form_captcha').attr('required', 'true');
                }
            }
            function siecomparte(sie){
                var formData = {sie: sie};
                if(sie.length >= 8){
                    $.ajax({
                        type: 'GET',
                        url: "{{path('tramite_rue_buscar_ruecomparte')}}",
                        data: formData,
                        beforeSend: function () {
                            $('#msgcomparte').empty();
                            $('#ie').empty();
                            $('#org').empty();
                            $('#dependencia').empty();
                        },
                        success: function (data) {
                            //console.log(data);
                            if (data.msg){
                                $("#msgcomparte").append('<div class="alert alert-danger"> <button type="button" class="close" data-dismiss="alert" aria-label="Close"><span aria-hidden="true">&times;</span></button>'+data.msg+'</div>');
                                $('#form_siecomparte').val('');
