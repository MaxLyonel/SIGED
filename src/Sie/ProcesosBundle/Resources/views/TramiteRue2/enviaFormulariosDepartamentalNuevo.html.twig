                return false;
            }else if($('#form_direccion').val()==''){
                alert('Debe ingrasar la dirección de la unidad educativa');
                return false;
            }else if($('#form_direccion').val()==''){
                alert('Debe ingrasar la dirección de la unidad educativa');
                return false;
            }else if($('#form_dependencia').val()==''){
                alert('Debe seleccionar el tipo de dependencia de la Unidda Educativa');
                return false;
            }else if($('#form_tipoeducacion').val()==''){
