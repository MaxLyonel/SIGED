                $("#form_formularios_2").attr('checked',false);
                $("#form_formularios_3").attr('checked',false);
                $("#form_varevaluacion1_0").attr('required',true);
                $("#form_varevaluacion1_1").attr('required',true);
                $("#form_varevaluacion1_0").attr('checked',false);
                $("#form_varevaluacion1_1").attr('checked',false);
