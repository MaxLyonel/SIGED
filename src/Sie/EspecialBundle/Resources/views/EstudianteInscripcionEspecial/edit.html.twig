{% block javascripts %}
    {{parent()}}
    <script src="{{asset('js/jquery-ui.min.js')}}"></script>
    <script>  

        /*  functiones para seleccionar nivles grados y paralelos */
        function provincia1(d) {
            $.ajax({
                type: 'get',
                url: Routing.generate('search_provincias', {'departamento': d}),
                beforeSend: function () {
                    $("#form_domicilioProvincia").empty();
                    $("#form_domicilioProvincia").append('<option value="">Buscando...</option>');
                },
                success: function (data) {
                    $("#form_domicilioProvincia").empty();
                    $("#form_domicilioProvincia").append('<option value="">Seleccionar...</option>');
                    $.each(data.provincia, function (i, value) {
                        $("#form_domicilioProvincia").append('<option value="' + i + '">' + value + '</option>');
                    });
                }
            });
        }

        function cargarAreas1() {
            var institucion = $('#form_idInstitucion').val();
            var gestion = $('#form_gestion').val();
            var turno = $('#form_turno').val();
            $.ajax({
                type: 'get',
                url: Routing.generate('cargar_areas_especial', {'idInstitucion': institucion,'gestion':gestion,'turno':turno}),
                beforeSend: function () {
                    $("#form_area").empty();
                	$("#form_nivel").empty();
                    $("#form_grado").empty();
                    $("#form_paralelo").empty();
                    $("#form_area").append('<option value="">Buscando...</option>');
                    $("#form_nivel").append('<option value="">Buscando...</option>');
                    $("#form_grado").append('<option value="">Buscando...</option>');
                    $("#form_paralelo").append('<option value="">Buscando...</option>');
                },
                success: function (data) {
                    $("#form_area").empty();
                	$("#form_nivel").empty();
                    $("#form_grado").empty();
                    $("#form_paralelo").empty();
                    $("#form_area").append('<option value="">Seleccionar...</option>');
                    $("#form_nivel").append('<option value="">Seleccionar...</option>');
                    $("#form_grado").append('<option value="">Seleccionar...</option>');
                    $("#form_paralelo").append('<option value="">Seleccionar...</option>');
                    $.each(data.areas, function (i, value) {
                        $("#form_area").append('<option value="' + i + '">' + value + '</option>');
                    });
                }
            });
        }

        function cargarNiveles1() {
            var institucion = $('#form_idInstitucion').val();
            var gestion = $('#form_gestion').val();
            var turno = $('#form_turno').val();
            var area = $('#form_idArea').val();
            $.ajax({
                type: 'get',
                url: Routing.generate('cargar_niveles_especial', {'idInstitucion': institucion,'gestion':gestion,'turno':turno,'area':area}),
                beforeSend: function () {
                    $("#form_nivel").empty();
                    $("#form_grado").empty();
                    $("#form_servicio").empty();
                    $("#form_programa").empty();
                    $("#form_paralelo").empty();
                    $("#form_nivel").append('<option value="">Buscando...</option>');
                    $("#form_grado").append('<option value="">Buscando...</option>');
                    $("#form_servicio").append('<option value="">Buscando...</option>');
                    $("#form_programa").append('<option value="">Buscando...</option>');
                    $("#form_paralelo").append('<option value="">Buscando...</option>');
                },
                success: function (data) {
                    $("#form_nivel").empty();
                    $("#form_grado").empty();
                    $("#form_servicio").empty();
                    $("#form_programa").empty();
                    $("#form_paralelo").empty();
                    $("#form_nivel").append('<option value="">Seleccionar...</option>');
                    $("#form_grado").append('<option value="">Seleccionar...</option>');
                    $("#form_servicio").append('<option value="">Seleccionar...</option>');
                    $("#form_programa").append('<option value="">Seleccionar...</option>');
                    $("#form_paralelo").append('<option value="">Seleccionar...</option>');
                    $.each(data.niveles, function (i, value) {
                        $("#form_nivel").append('<option value="' + i + '">' + value + '</option>');
                    });
                }
            });
        }
        
        function cargarGrados1() {
            var institucion = $('#form_idInstitucion').val();
            var gestion = $('#form_gestion').val();
            var turno = $('#form_turno').val();
            var area = $('#form_idArea').val();
            var nivel = $('#form_nivel').val();
            $.ajax({
                type: 'get',
                url: Routing.generate('cargar_grados_especial', {'idInstitucion': institucion,'gestion':gestion,'turno':turno,'area':area,'nivel':nivel}),
                beforeSend: function () {
                    $("#form_grado").empty();
                    $("#form_servicio").empty();
                    $("#form_programa").empty();
                    $("#form_paralelo").empty();
                    $("#form_grado").append('<option value="">Buscando...</option>');
