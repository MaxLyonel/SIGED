                url: Routing.generate('info_estudiante_rudees_nuevo_cargar_grado_discapacidad', {'discapacidad': discapacidad}),
                beforeSend: function () {
                    $("#form_gradoDiscapacidad").empty();
                    $("#form_gradoDiscapacidad").append('<option value="">Seleccionar...</option>');
                },
                success: function (data) {
                    $("#form_gradoDiscapacidad").empty();
                    $("#form_gradoDiscapacidad").append('<option value="">Seleccionar...</option>');

                    $.each(data.gradosDiscapacidad, function (i, value) {
                        $("#form_gradoDiscapacidad").append('<option value="' + i + '">' + value + '</option>');
                    });
                }
            });
