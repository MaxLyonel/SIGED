{% extends 'layoutRegular.html.twig' %}

{% block contentoption %}

    <!-- begin PAGE TITLE AREA -->
    <!-- Use this section for each page's title and breadcrumb layout. In this example a date range picker is included within the breadcrumb. -->

    <div class="row">
        <div class="col-lg-12">
            <div class="page-title">
                <h1>Control y Validación
                    <small>Estudiante</small>
                </h1>
                <ol class="breadcrumb">
                    <li class="active"><i class="fa fa-edit"></i> Control y Validación</li>
                </ol>
            </div>
        </div>
        <!-- /.col-lg-12 -->
    </div>
    <!-- /.row -->

    <div class="row">
        <div class="col-lg-6 col-md-6 col-sm-12 col-xs-12">
            <div class="row">
                <div class="col-lg-12 col-md-12">
                    <div class="portlet portlet-blue">
                        <div class="portlet-heading">
                            <div class="portlet-title">
                                <h4><i class="fa fa-user"></i> Datos</h4>
                            </div>
                            <div class="clearfix"></div>
                        </div>
                        <div class="portlet-body">
                            <div class="table-responsive">
                                <table class="table table-bordered table-striped table-hover">
                                    <tbody>
                                        <tr>
                                            <td>Apellido Paterno:</td>
                                            <td>{{ app.session.get('lastname') }}</td>
                                        </tr>
                                        <tr>
                                            <td>Apellido Materno:</td>
                                            <td>{{ app.session.get('lastname2')}}</td>
                                        </tr>
                                        <tr>
                                            <td>Nombre(s):</td>
                                            <td>{{ app.session.get('name') }}</td>
                                        </tr>
                                        <tr>
                                            <td>Usuario:</td>
                                            <td>{{ app.session.get('userName') }}</td>
                                        </tr>
                                        <tr>
                                            <td>Cuenta:</td>
                                            <td>{{ app.session.get('cuentauser') }}</td>
                                        </tr>
                                </table>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>

        <div class="col-lg-6 col-md-6 col-sm-12 col-xs-12">
            <div class="row">
                <div class="col-lg-12 col-md-12">
                    <div class="portlet portlet-blue">
                        <div class="portlet-heading">
                            <div class="portlet-title">
                                <h4><i class="fa fa-user"></i> Datos</h4>
                            </div>
                            <div class="clearfix"></div>
                        </div>
                        <div class="portlet-body">
                            <div class="table-responsive">
                                <table class="table table-bordered table-striped table-hover">
                                    <tbody>
                                        <tr>
                                            <td>Apellido Paterno:</td>
                                            <td>{{ app.session.get('lastname') }}</td>
                                        </tr>
                                        <tr>
                                            <td>Apellido Materno:</td>
                                            <td>{{ app.session.get('lastname2')}}</td>
                                        </tr>
                                        <tr>
                                            <td>Nombre(s):</td>
                                            <td>{{ app.session.get('name') }}</td>
                                        </tr>
                                        <tr>
                                            <td>Usuario:</td>
                                            <td>{{ app.session.get('userName') }}</td>
                                        </tr>
                                        <tr>
                                            <td>Cuenta:</td>
                                            <td>{{ app.session.get('cuentauser') }}</td>
                                        </tr>
                                </table>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>

{% endblock %}
