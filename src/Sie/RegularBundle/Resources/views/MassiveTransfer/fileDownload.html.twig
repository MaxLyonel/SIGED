<div class="row">
    <div class="col-lg-12">
        <div class="portlet portlet-basic">
            <div class="portlet-body">

                <div class="row ">

                    <div class="col-lg-12">

                        <div class="portlet portlet-basic">
                            <div class="portlet-body">
                                <!--h3 id="grid-offsetting">Datos Estudiantes</h3-->
                                <div class="small">
                                    <div id="no-more-tables">
                                        {{form_start(form, { 'attr': {'data-toggle': 'validator','role': "form",'class':'form', 'id':'formdesuni'} } )}}
                                        <div class="row">
                                            <div class="col-md-4 ">
                                                <div class="alert alert-danger alert-dismissible" role="alert">
                                                    <strong><i class='fa fa-linux'></i></strong> 
                                                    Registre el SIE y seleccione los estudiantes para la transferencia 
                                                </div>
                                            </div>
                                            <div class="col-md-4 ">
                                                <label for="">{{form_label(form.sienew)}}</label>
                                                {{form_widget(form.sienew )}}
                                            </div>
                                            <div class="col-md-4">
                                                <p></p>
                                                {{ form_widget(form.massivetransfer) }}

                                            </div>

                                        </div>
                                        <table class="table table-bordered table-hover cf">
                                            <thead class="cf">
                                                <tr class="info">
                                                    <th class="text-center">No.</th>
                                                    <th class="text-center">Cod. Rude</th>
                                                    <th class="text-center">CI</th>
                                                    <th class="text-center">Paterno</th>
                                                    <th class="text-center">Materno</th>
                                                    <th class="text-center">Nombre</th>
                                                    <th class="text-center">&nbsp;</th>
                                                </tr>
                                            </thead>
                                            <tbody>
                                                {%for student in students %}
                                                    <tr><td>{{loop.index}}</td>
                                                        <td data-title="Asignatura"> {{student.codigoRude}}</td>
                                                        <td data-title="Asignatura"> {{student.carnetIdentidad}}</td>
                                                        <td data-title="Asignatura"> {{student.paterno}}</td>
                                                        <td data-title="Asignatura"> {{student.materno}}</td>
                                                        <td data-title="Asignatura"> {{student.nombre}}</td>
                                                        <td data-title="Opcion" class="success" >
                                                            <div class="checkbox">
                                                                <label>
                                                                    <input type="checkbox" id="forme_datasend_{{loop.index}}" name="forme[datasend_{{loop.index}}]"   value='{{student.studentInscId}}'>
                                                                </label>
                                                            </div>

                                                        </td>
                                                    </tr>
                                                {% endfor%}
                                            </tbody>
                                        </table>
                                        {{form_end(form)}}
                                    </div>
                                </div>
                            </div>
                        </div>


                    </div>



                </div>

            </div>
        </div>
    </div>
    <!-- /.col-lg-12 -->
</div>
<!-- /.row -->