{% extends 'layoutRegular.html.twig' %}

{% block contentoption -%}

    <div class="row">
        <div class="col-lg-12">
            <div class="page-title">
                <h1>Unidad Educativa Humanistico Técnico 
                    <small></small>
                </h1>
                <ol class="breadcrumb">
                    <li><i class="fa fa-dashboard"></i>  <a href="#">Lista</a></li>

                </ol>
            </div>
        </div>
        <!-- /.col-lg-12 -->
    </div>
    <div class="row">

        <div class="col-lg-12">

            <div class="portlet portlet-default">
                <div class="portlet-heading">
                    <div class="portlet-title">
                        <h4>Listado</h4>

                    </div>
                    <div class="text-right">

                        <a href="{{ path('sie_ueHumTec_new') }}"  class="btn btn-success btn-xs ladda-button" data-style="expand-right" data-size="xs">
                            <span class="ladda-label">Nueva U.E.</span>
                            <span class="ladda-spinner"></span>
                        </a>
                    </div>
                    <div class="clearfix"></div>
                </div>
                <div class="portlet-body">
                    <div class="table-responsive">
                        <div id="example-table_wrapper" class="dataTables_wrapper form-inline" role="grid">

                            <table id="myTable" class="table table-bordered table-hover cf dataTable">
                                <thead>
                                    <tr>
                                        <th>No</th>
                                        <th>Gestiontipoid</th>
                                        <th>Institucioneducativaid</th>
                                        <th>Institucioneducativa</th>
                                        <th>Opción</th>
                                        <!--th>Opción</th-->
                                    </tr>
                                </thead>
                                <tbody>
                                    {% for entity in entities %}
                                        <tr>
                                            <td>{{loop.index}}</a></td>
                                            <td>{{ entity.gestionTipoId }}</td>
                                            <td>{{ entity.institucioneducativaId }}</td>
                                            <td>{{ entity.institucioneducativa }}</td>
                                            <!--td>
                                                <a href="{{ path('sie_ueHumTec_show', { 'id': entity.id }) }}" class="btn btn-link btn-xs ladda-button" data-style="expand-right" data-size="xs">
                                                    <span class="ladda-label"> <i class="fa fa-eye"></i> Ver</span>
                                                    <span class="ladda-spinner"></span>
                                                </a>
                                            </td-->
                                            <td>
                                                <a href="{{ path('sie_ueHumTec_edit', { 'id': entity.id }) }}" class="btn btn-link btn-xs ladda-button" data-style="expand-center" data-size="xs">
                                                    <span class="ladda-label"> <i class="fa fa-edit"></i> Editar</span>
                                                    <span class="ladda-spinner"></span>
                                                </a>
                                            </td>
                                        </tr>
                                    {% endfor %}
                                </tbody>
                            </table>

                        </div>
                    </div>
                    <!-- /.table-responsive -->
                </div>
                <!-- /.portlet-body -->
            </div>
            <!-- /.portlet -->

        </div>
        <!-- /.col-lg-12 -->
    </div>

{% endblock %}
