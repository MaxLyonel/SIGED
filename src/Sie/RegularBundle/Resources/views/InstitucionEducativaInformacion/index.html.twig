                                            <td data-title="#">{{ ueMul.multigrado }}</td>
                                            <td data-title="Ini. 1°">{{ ueMul.ini1 }}</td>
                                            <td data-title="Ini. 2°">{{ ueMul.ini2 }}</td>
                                            <td data-title="Pri. 1°">{{ ueMul.pri1 }}</td>
