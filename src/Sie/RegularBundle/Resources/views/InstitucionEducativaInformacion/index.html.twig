                                            <td data-title="Pri. 3°">{{ ueMul.pri3 }}</td>
                                            <td data-title="Pri. 4°">{{ ueMul.pri4 }}</td>
                                            <td data-title="Pri. 5°">{{ ueMul.pri5 }}</td>
                                            <td data-title="Pri. 6°">{{ ueMul.pri6 }}</td>
                                            <td data-title="Sec. 1°">{{ ueMul.sec1 }}</td>
