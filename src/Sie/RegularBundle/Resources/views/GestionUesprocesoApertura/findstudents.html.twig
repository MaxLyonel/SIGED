<small>
{%if exist %}
<div class="col-lg-12">
                        <div class="portlet portlet-basic">
                            <div class="portlet-body">
                                <h5>Resultado de la Busqueda</h5>

                                <div class="row show-grid">
                                    <div class="col-xs-4 col-sm-6">Nombre: </br>{{student.paterno }} {{student.materno}} {{student.nombre}}</div>
                                    <div class="col-xs-4 col-sm-3">CI: </br>{{student.carnetIdentidad}}</div>
                                    <div class="col-xs-4 col-sm-3">Fec. Nacimiento: </br> {{student.fechaNacimiento | date('d-m-Y')}}</div>
                                </div>


                                <div class="table-responsive">
                                    <table id="datatable-ajax" class="table table-striped table-success dataTable" role="grid" aria-describedby="datatable-ajax_info">
                                      <thead>
                                          <tr role="row">
                                            <th >Gestión</th>
                                            <th >Sie</th>
                                            <th >Nivel</th>
                                            <th >Grado</th>
                                            <th >Paralelo</th>
                                            <th >Turno</th>
                                            <th >Estado</th>
                                          </tr>
                                      </thead>
                                      <!--tbody section is required-->
                                      <tbody>
                                        {#init the array info student #}
                                      {% if objStudent %}
                                      {% for data in objStudent %}
                                        {# set the array student info and convert on json string #}

                                        <tr role="row" class="odd">
                                            <td >{{data.gestionTipo}}</td>
                                            <td >{{data.ueId}}</td>
                                            <td>{{data.nivel}}</td>
                                            <td>{{data.grado}}</td>
                                            <td>{{data.paralelo}}</td>
                                            <td>{{data.turno}}</td>
                                            <td>{{data.estadoMatricula}}</td>
                                            <!--td  align="">
                                                <div class="btn-group tooltips" data-toggle="tooltip" data-container="body" title="" data-original-title="Calificaciones">
                                                    <a href="#" class="" data-toggle="modal" data-target=".bs-example-modal-table" onclick="doInscriptionByStudent('{{ data.id|json_encode(data.id) }}', '{{dataUe}}' );" >
                                                       Inscribir
                                                    </a>
                                                </div>
                                            </td-->
                                        </tr>
                                      {% endfor %}
                                      {% else %}
                                      <tr role="row" class="odd" >
                                          <td  align="center" colspan='7'>
                                              <--  No existe resultado de la busqueda -->
                                          </td>
                                      </tr>

                                      {% endif %}
                                      </tbody>
                                      <!--tfoot section is optional-->
                                      <tfoot>
                                      </tfoot>
                                    </table>
                                </div>
                            </div>
                        </div>
                    </div>
  <div class='row'>
    <div class='col-md-4 col-md-offset-8'></div>
  </div>

{% else %}
<div class='row'>
  <div class="col-md-12">

  {% for flashMessage in app.session.flashbag.get('noresult') %}
      <div class="alert alert-warning alert-dismissible" >
          <button type="button" class="close" data-dismiss="alert" aria-hidden="true">×</button>
          <strong>Alerta!</strong> {{ flashMessage }}
      </div>
  {% endfor %}

  <div class="pull-right">
      {{form_start(form, { 'attr': {'data-toggle': 'validator','role': "form", 'id':'idInscriptionFormNoAcredit', 'class':'form-inline'} } )}}
      {{form_end(form)}}
  </div>
  </div>
</div>
{% endif %}
</small>
