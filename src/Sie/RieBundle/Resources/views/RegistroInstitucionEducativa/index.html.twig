{% extends '::base.html.twig' %}

{% block body -%}
    <h1>Institucioneducativa list</h1>

    <table class="records_list">
        <thead>
            <tr>
                <th>Id</th>
                <th>Institucioneducativa</th>
                <th>Rueue</th>
                <th>Fecharesolucion</th>
                <th>Nroresolucion</th>
                <th>Obsrue</th>
                <th>Desueantes</th>
                <th>Fechacreacion</th>
                <th>Fechacierre</th>
                <th>Actions</th>
            </tr>
        </thead>
        <tbody>
        {% for entity in entities %}
            <tr>
                <td><a href="{{ path('institucioneducativa_show', { 'id': entity.id }) }}">{{ entity.id }}</a></td>
                <td>{{ entity.institucioneducativa }}</td>
                <td>{{ entity.rueUe }}</td>
                <td>{% if entity.fechaResolucion %}{{ entity.fechaResolucion|date('Y-m-d H:i:s') }}{% endif %}</td>
                <td>{{ entity.nroResolucion }}</td>
                <td>{{ entity.obsRue }}</td>
                <td>{{ entity.desUeAntes }}</td>
                <td>{% if entity.fechaCreacion %}{{ entity.fechaCreacion|date('Y-m-d H:i:s') }}{% endif %}</td>
                <td>{{ entity.fechaCierre }}</td>
                <td>
                <ul>
                    <li>
                        <a href="{{ path('institucioneducativa_show', { 'id': entity.id }) }}">show</a>
                    </li>
                    <li>
                        <a href="{{ path('institucioneducativa_edit', { 'id': entity.id }) }}">edit</a>
                    </li>
                </ul>
                </td>
            </tr>
        {% endfor %}
        </tbody>
    </table>

        <ul>
        <li>
            <a href="{{ path('institucioneducativa_new') }}">
                Create a new entry
            </a>
        </li>
    </ul>
    {% endblock %}
