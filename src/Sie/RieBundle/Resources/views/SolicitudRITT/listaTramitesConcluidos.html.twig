{% extends 'SieRieBundle:Default:rie.html.twig' %}

{% block contentoption %}
    <div class="block-header">
        <h2></h2>
    </div>

    <div class="row clearfix">
        <div class="col-lg-12 col-md-12 col-sm-12 col-xs-12">
            <div class="card">
                <div class="header bg-blue-grey">
                    <h2>
                        Trámites Concluidos
                    </h2>
                </div>
                <div class="body">
                    <div class="row">
                    </div>
                    <div class="table-responsive">
                        <div class="col-lg-12 col-md-12 col-sm-12 col-xs-12">
                            <!--<table class="table table-bordered table-striped table-hover js-exportable dataTable">-->
                            <table class="table table-striped table-hover js-basic-example dataTable">
                                <thead>
                                <tr>
                                    <th WIDTH="10%">Nro.</th>
                                    <th>Depto.</th>
                                    <th>Cod. RITT</th>
                                    <th>Denominación</th>
                                    <th>Nro. Trámite</th>
                                    <th>Fecha Concluido</th>
                                    <th>Estado</th>
                                </tr>
                                </thead>
                                <tfoot>
                                <tr>
                                    <th  WIDTH="10%">Nro.</th>
                                    <th>Depto.</th>
                                    <th>Cod. RITT</th>
                                    <th>Denominación</th>
                                    <th>Nro. Trámite</th>
                                    <th>Fecha de Concluido</th>
                                    <th>Estado</th>
                                </tr>
                                </tfoot>
                                <tbody>
                                {% for tramite in listaTramites %}
                                    <tr>
                                        <td  WIDTH="10%">{{ loop.index }}</td>
                                        <td>{{ tramite.lugar }}</td>
                                        <td>{{ tramite.codrie}}</td>
                                        <td>{{ tramite.institucioneducativa}}</td>
                                        <td>{{ tramite.id }}</td>
                                        <td class="text-center">{{ tramite.fecha_fin }}</td>
                                        <td>{{ tramite.estado }}</td>
                                    </tr>
                                {% endfor %}
                                </tbody>
                            </table>


                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>




{% endblock %}
{% block javascripts %}
    {{parent()}}

<script type="text/javascript">
    function confirmar(){
        if(confirm('¿Estas segura/o de finalizar  el trámite e Imprimir el certificado?'))
        { setTimeout(function(){ location.reload();  },5000);
           return true;
        }
        else
            return false;

    }
    $(".alert").fadeTo(3000, 1000).slideUp(1000, function(){
        $(".alert").slideUp(1000);
    });
</script>
{% endblock %}