                    {% if formBusqueda is defined  %}     
                        {{form_start(formBusqueda, { 'attr': {'data-toggle': 'validator','role': "form", 'class': 'form-horizontal'} } )}}
                            <div class="form-body">
                                <div class="form-group">
                                    <label class="col-sm-3 control-label">{{ form_label(formBusqueda.serie) }} </label>
                                    <div class="col-sm-7">
                                        {{ form_widget(formBusqueda.serie) }}
                                    </div>
                                </div><!-- /.form-group -->
                                <div class="form-group">
                                    <label class="col-sm-3 control-label">{{ form_label(formBusqueda.obs) }} </label>
                                    <div class="col-sm-7">
                                        {{ form_widget(formBusqueda.obs) }} 
                                    </div>
                                </div><!-- /.form-group -->
                            </div><!-- /.form-body -->
                            <div class="form-footer">
                                <div class="col-sm-12">
                                    {{ form_widget(formBusqueda.search,{ 'attr':{'class':'btn btn-primary mb-15 btn-block' } } ) }}
                                </div>
                            </div><!-- /.form-footer -->
                        {{form_end(formBusqueda)}}
                    {% endif %}
                </div><!-- /.panel-body -->
