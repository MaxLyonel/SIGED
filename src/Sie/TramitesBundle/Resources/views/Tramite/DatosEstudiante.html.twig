        <script>

            var datetime = null,
                    date = null;
            var update = function () {
                date = moment(new Date());
                date = date.locale('fr');
                datetime.html(date.format('dddd<br>D [de] MMMM [de] YYYY<br>h:mm:ss A'));
            };
            $(document).ready(function () {
                $('#myModal').modal('show');
                datetime = $('#datetime');
                update();
                setInterval(update, 1000);
            {#                    $("#form_especialidad").attr('disabled', 'true');
                                $("#form_nivel").attr('disabled', 'true');#}
                                    });
                                    $(document).ready(function () {
                                        datetoday = new Date(); // create new Date()
                                        timenow = datetoday.getTime(); // grabbing the time it is now
                                        datetoday.setTime(timenow); // setting the time now to datetoday variable
                                        hournow = datetoday.getHours(); //the hour it is

                                        if (hournow >= 18) // if it is after 6pm
                                            $('div.tile-img').addClass('evening');
                                        else if (hournow >= 12) // if it is after 12pm
                                            $('div.tile-img').addClass('afternoon');
                                        else if (hournow >= 6) // if it is after 6am
                                            $('div.tile-img').addClass('morning');
                                        else if (hournow >= 0) // if it is after midnight
                                            $('div.tile-img').addClass('midnight');
                                    });
                                    function num_mayor() {
                                        var inic = parseInt(document.getElementById('form_num_inicial').value);
                                        var fin = parseInt(document.getElementById('form_num_final').value);

                                        if (fin < inic) {
                                            alert('El número Inicial ' + inic + ' debe ser MENOR o IGUAL al Número Final ' + fin);
                                            document.getElementById('form_num_final').value = '';
                                            $('#form_num_final').focus();
                                        }
                                    }
                                    function verificarSelect() {
                                        var c = 0;
                                        var series = $('#form_series');
                                        var gestiones = $('#form_gestiones');
                                        var depto = $('#form_departamento');
                                        var inicial = $('#form_num_inicial');
                                        var final = $('#form_num_final');
                                        var niveles = $('#form_niveles');
                                        var boton = $('#form_verificarBtn');
                                        if (series.val() == '') {
                                            series.css('border', '1px solid red');
                                            c = c + 1;
                                        } else {
                                            series.css('border', '1px solid #8cc152');
                                        }
                                        if (gestiones.val() == '') {
                                            gestiones.css('border', '1px solid red');
                                            c = c + 1;
                                        } else {
                                            gestiones.css('border', '1px solid #8cc152');
                                        }
                                        if (depto.val() == '') {
                                            depto.css('border', '1px solid red');
                                            c = c + 1;
                                        } else {
                                            depto.css('border', '1px solid #8cc152');
                                        }
                                        if (inicial.val() == '') {
                                            inicial.css('border', '1px solid red');
                                            c = c + 1;
                                        } else {
                                            inicial.css('border', '1px solid #ccc');
                                        }
                                        if (final.val() == '') {
                                            final.css('border', '1px solid red');
                                            c = c + 1;
                                        } else {
                                            final.css('border', '1px solid #ccc');
                                        }
                                        if (niveles.val() == '') {
                                            niveles.css('border', '1px solid red');
                                            c = c + 1;
                                        } else {
                                            niveles.css('border', '1px solid #8cc152');
                                        }
                                        if (c == 0) {
                                            $('#form_verificarBtn').css('transition', '0.5s all ease-in-out');
                                            $('#form_verificarBtn').removeClass('disabled');
                                            $('#form_verificarBtn').removeAttr('disabled');
                                        }
                                    }
