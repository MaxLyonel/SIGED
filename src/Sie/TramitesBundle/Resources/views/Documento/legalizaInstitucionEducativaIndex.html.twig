{% block contentoption %}
    <div class="row">
        <div class="col-lg-12 col-md-12 col-sm-12">
            <div class="panel panel-default">
                <div class="panel-body">
                    {% if form is defined  %}
                        {{form_start(form, { 'attr': {'data-toggle': 'validator','role': "form", 'class': 'form-horizontal'} } )}}
                            <div class="form-body">
                                <div class="form-group">
                                    <label class="col-sm-3 control-label">{{ form_label(form.sie) }} </label>
                                    <div class="col-sm-7">
                                        {{ form_widget(form.sie) }}
                                    </div>
                                </div><!-- /.form-group -->
                                <div class="form-group">
                                    <label class="col-sm-3 control-label">{{ form_label(form.gestion) }} </label>
                                    <div class="col-sm-7">
                                        {{ form_widget(form.gestion) }}
                                    </div>
                                </div><!-- /.form-group -->
                            </div><!-- /.form-body -->
                            <div class="form-footer">
                                <div class="col-sm-12">
                                    {{ form_widget(form.search,{ 'attr':{'class':'btn btn-primary mb-15 btn-block' } } ) }}
                                </div>
                            </div><!-- /.form-footer -->
                        {{form_end(form)}}
                    {% endif %}
                </div><!-- /.panel-body -->
            </div>
        </div>
