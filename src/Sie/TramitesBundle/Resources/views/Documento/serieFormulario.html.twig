<div class="modal-dialog">
    <div class="modal-content">
        <!-- Start page header -->
        <div class="modal-header">
            <button type="button" class="close" data-dismiss="modal" aria-hidden="true">Ã—</button>
            <h4><i class="fa fa-table"></i> Formulario <span>Nuevo numero y serie</span></h4>
        </div><!-- /.header-content -->
        <!--/ End page header -->
        <div class="modal-body">
            {% if form is defined  %}
                {{form_start(form, { 'attr': {'id':'form', 'data-toggle': 'validator','role': "form", 'class': 'form-horizontal'} } )}}
                    <div class="form-body">
                        <div class="form-group">
                            {{ form_widget(form.departamento) }}
                        </div>  
                        <div class="form-group">
                            {{ form_widget(form.gestion) }}
                        </div>  
                        <div class="form-group">
                            {{ form_widget(form.documentoTipo) }}
                        </div>  
                        <div class="form-group">
                            {{ form_widget(form.numInicial) }}
                        </div>  
                        <div class="form-group">
                            {{ form_widget(form.numFinal) }}
                        </div>  
                        <div class="form-group">
                            {{ form_widget(form.serie) }}
                        </div>  
                        <div class="form-group">
                            {{ form_widget(form.obs) }}
                        </div>  
                        <div class="form-group">
                            {{ form_widget(form.guardar,{ 'attr':{'class':'btn btn-primary col-lg-12 col-md-12 col-sm-12 col-xs-12' } } ) }}
                        </div>
                    </div>
                {{form_end(form)}} 
            {% endif %}
        </div> 
    </div>    
</div>
                
       