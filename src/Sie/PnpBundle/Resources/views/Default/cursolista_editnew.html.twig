{% for datos in datosentity %}
    <div class="body-content animated fadeIn">
        <div class="row">
            <div class="col-xs-12 col-lg-12">
                <div class="panel panel-icon panel-primary">
                    <div class="panel-heading">
                        Datos Generales del Curso
                    </div>
                    <div class="panel-body">
                        <div class="row">
                            <div class="col-sm-4">
                                <h5><small>DEPARTAMENTO :</small><br>{{ datos.departamento }}</h5>
                            </div>
                            <div class="col-xs-4">
                                <h5><small>FACILITADOR :</small><br>{{ datos.nombre }} {{ datos.paterno }} {{ datos.materno }}</h5>                    
                            </div>
                            <div class="col-xs-4">
                                <h5><small>CI. DEL FACILITADOR :</small><br>{{ datos.carnet }}{% if datos.complemento != '' %}-{{datos.complemento}}{% endif %}</h5>                        
                            </div>
                        </div>
                        <div class="row">
                            <div class="col-xs-4">
                                <h5><small>PROVINCIA :</small><br>{{ datos.provincia }}</h5>                    
                            </div>
                            <div class="col-xs-4">
                                <h5><small>FECHA DE INICIO :</small></small><br>{% if datos.fecha_inicio %}{{ datos.fecha_inicio|date('d/m/Y') }}{% endif %}</h5>
                            </div>
                            <div class="col-md-4">
                                <h5><small>CICLO :</small><br>{% if plan == 1 %}BLOQUE {% endif %}{{ datos.nciclo }}</h5>
                                <input hidden="true" id="bloque" value="{{ datos.ciclo_tipo_id }}"></input>                   
                            </div>
                        </div>
                        <div class="row">
                            <div class="col-md-4">
                                <h5><small>MUNICIPIO :</small><br>{{ datos.municipio }}</h5>                   
                            </div>
                            <div class="col-md-4">
                                <h5><small>FECHA DE FIN :</small><br>{% if datos.fecha_fin %}{{ datos.fecha_fin|date('d/m/Y') }}{% endif %}</h5>
                            </div>
                            <div class="col-xs-4">
                                <h5><small>GRADO:{% if plan == 2 %} ÁMBITO DE APRENDIZAJE:{% endif %}</small><br>{% if plan == 1 %}PARTE {% endif %}{{ datos.ngrado }} {% if datos.grado_tipo_id == 14 or datos.grado_tipo_id == 16 %} (Semestre 1) {% elseif datos.grado_tipo_id == 15 or datos.grado_tipo_id == 17 %} (Semestre 2) {% endif %}</h5>
                                <input hidden="true" id="parte" value="{{ datos.grado_tipo_id }}"></input>
                            </div>
                        </div>
                        <div class="row">
                            <div class="col-sm-4">
                                <h5><small>LOCALIDAD :</small><br>{{ datos.localidad }}</h5>
                            </div>
                            <div class="col-xs-4">
                                <h5><small>GESTIÓN:</small><br>{{ datos.gestion_tipo_id }}</h5>
                            </div>
                            <div class="col-sm-4">
                                <h5><small>NRO. MATERIAS :</small><br>{{ datos.nro_materias }}</h5>
                            </div>
                        </div>
                        <div class="row">    
                            <div class="col-xs-4">
                                <h5><small>CÓDIGO RUE :</small><br>{{ datos.institucioneducativa_id }}</h5>                    
                            </div>
                            <div class="col-xs-4">
                                <h5><small>ID DEL ARCHIVO CONSOLIDADO :</small><br>{{ datos.id }}</h5>
                                <input hidden="true" id="curso_id" value="{{ datos.id }}"></input>        
                            </div>
                            {% if plan == 2 %}
                                <div class="col-xs-4">
                                    <h5><small>FORMACIÓN PARA LA VIDA:</small><br>{{modulo_emergente.moduloEmergente}}</h5>
                                </div>
                            {% endif %}
                        </div>
                    </div>
                </div>
                <div class="panel panel-icon panel-primary">
                    <div class="panel-heading">
                        Lista de Participantes del Curso
                    </div>
                    <div class="panel-body">
                        <div class="contenedor-tabla" style="width: 100%; overflow: scroll;">
                            <table id="tablaListadoPersonas" class="display" cellspacing="0" width="100%">    
                                <thead>
                                    <tr>
                                        <th>Codigo Rude</th>
                                        {% if plan == 2 %}<th>Rudeal</th>{% endif %}
                                        <th>Opciones</th>
                                        <th>Carnet Identidad</th>
                                        <th>Paterno</th>
                                        <th>Materno</th>
                                        <th>Nombre</th>
                                        <th>Matricula:</th>
                                        <th>Fecha de Nacimiento</th>
                                        <th>Género</th>
                                        <th>Alfabetizado|Idioma|Cultura</th>
                                        {% if plan == 2 %}<th>Discapacidad</th>{% endif %}
                                    </tr>
                                </thead>
                                <tbody>
                                {% for estudiante in estudiantes %}
                                    <tr>
                                        <td>{{ estudiante.codigo_rude }}</td>
                                        {% if plan == 2 %}
                                            <td>
                                                {% if esactivo == 1 %}
                                                    <a role="button" class="btn btn-xs btn-primary glyphicon glyphicon-print" href="{{ path('sie_pnp_imprimir_boletaelectronica', { 'id': estudiante.inscripcion_id~"|"~datos.ciclo_tipo_id~"|"~datos.grado_tipo_id}) }}" title="Imprimir Rudeal"></a>    
                                                {% else %}
                                                    <a role="button" class="btn btn-xs btn-primary glyphicon glyphicon-print" href="{{ path('sie_pnp_imprimir_boletaelectronica', { 'id': estudiante.inscripcion_id~"|"~datos.ciclo_tipo_id~"|"~datos.grado_tipo_id}) }}" title="Imprimir Rudeal"></a>    
                                                    <a href="#" role="button" onclick="SeeRudeal('{{estudiante.inscripcion_id}}')" class="btn btn-xs btn-success glyphicon glyphicon-edit" title="Editar Rudeal"></a>
                                                {% endif %}    
                                            </td>
                                        {% endif %}
                                        <td>
                                        {% if esactivo == 1 %}
                                            <a role="button" class="btn btn-xs btn-primary glyphicon glyphicon-print" href="{{ path('sie_pnp_imprimir_boletaelectronica', { 'id': estudiante.inscripcion_id~"|"~datos.ciclo_tipo_id~"|"~datos.grado_tipo_id}) }}" title="Libreta Electronica"></a>
                                            <a href="#" role="button" title="Ver Calificaciones" onclick="SeeNotasNoEdit('{{estudiante.inscripcion_id}}',{{ datos.id }})" class="glyphicon glyphicon-list btn-xs btn-info"></a>
                                        {% else %}
                                            <a href="#" role="button" onclick="SeeNotas('{{estudiante.inscripcion_id}}',{{ datos.id }})" class="btn btn-xs btn-primary glyphicon glyphicon-list" title="Editar Calificaciones"></a>
                                            <a href="#" role="button" onclick="FechaNacEdit('{{estudiante.estudiante_id}}','{{estudiante.fecha_nacimiento|date("d/m/Y")}}','{{ estudiante.id_genero }}','{{estudiante.observacionadicional}}',{{estudiante.pnp}},'{{estudiante.discapacidad_id}}')" class="btn btn-xs btn-success btn-large glyphicon glyphicon-edit" title="Editar Datos"></a>
                                            <a role="button" class="btn btn-danger btn-xs glyphicon glyphicon-remove" onclick="return confirm('¿Está seguro de elminar este registro?, Si ELIMINA el registro, se perderá todos los datos, como la INSCRIPCIÓN y las NOTAS del estudiante y no podrá recuperar!!!')" href="{{ path('sie_pnp_eliminarregistro_estudiante_curso', { 'estudiante_inscripcion_id': estudiante.inscripcion_id,'curso_id':datos.id }) }}" title="Eliminar Inscipción"></a>
                                        {% endif %}
                                        </td>
                                        <td>{{ estudiante.carnet_identidad }}{% if estudiante.complemento != '' %}-{{estudiante.complemento}}{% endif %}</td>
                                        <td>{{ estudiante.paterno }}</td>
                                        <td>{{ estudiante.materno }}</td> 
                                        <td>{{ estudiante.nombre }}</td>
                                        {% if estudiante.estadomatricula == 62 %}
                                            <td>Promovida/o</td>
                                        {% elseif estudiante.estadomatricula == 61  %}
                                            <td>Retirada/o</td>
                                        {% else %}
                                            <td>Postergada/o</td>
                                        {% endif %}
                                        
                                        <td>{% if estudiante.fecha_nacimiento %}{{ estudiante.fecha_nacimiento|date('d/m/Y') }}{% endif %}</td>
                                        <td>{{ estudiante.genero }}</td>
                                        <td>{{ estudiante.observacionadicional }}</td>
                                        {% if plan == 2 %}<td>{{ estudiante.discapacidad_nombre }}</td>{% endif %}
                                    </tr>
                                {% endfor %}
                                </tbody>
                            </table> 
                            {% endfor %}
                            <hr/>
                                {% if esactivo == 0 %}
                                    <table> 
                                        <tr>
                                            <td><a role="button" onclick="SeeBuscarEstudiante()" class="btn btn-primary" href="#" >Agregar Estudiante</a><br/><br/></td>
                                            {% if plan == 2 %}
                                                <td>
                                                    <a role="button" onclick="SeeModuloEmergente()" class="btn btn-warning" href="#" >Formación Para La Vida</a><br/><br/>
                                                </td>
                                                {% endif %}
                                            <td VALIGN="TOP">
                                                <a role="button" class="btn btn-success" href="{{ path('sie_pnp_curso_listado_editnew', { 'id': id_archivo , 'val':6 }) }}" onclick="return confirm('Está seguro de CERRAR EL CURSO?, si cierra el curso POR TEMAS DE SEGURIDAD SE GUARDARÁ SU NOMBRE, ya no tendrá las opciones de editar; Pero podrá imprimir el PDF original para sus firmas respectivas. No olvide que 10 estudiantes mínimo para áreas concentradas y 8 para desconcentradas.');" >Cerrar Curso</a>
                                            </td>
                                        </tr>
                                    </table>
                                {% else %}
                                    <div class="alert alert-success" role="alert" align="center"><strong>Curso Cerrado</strong></div>
                                {% endif %}
                            </div>
                        </div>
                    </div>
                </div>
            </div>
                        
  

    
    

{% endblock %}

