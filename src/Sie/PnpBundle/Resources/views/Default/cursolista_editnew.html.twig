                            {% if plan == 2%}
                                <div class="col-lg-3 col-md-12">&nbsp;</div>
                                <div class="col-lg-6 col-md-12" align="center">
                                    <h5>Tiene Discapacidad</h5> 
                                    <select name="discapacidad" class="form-control" id="discapacidad">
                                        {% for discapacidad in discapacidades %}
                                            <option value="{{discapacidad.id}}">{{discapacidad.origendiscapacidad}}</option>
