                                    <option value="MACHINERI">MACHINERI</option>
                                    <option value="MORÉ">MORÉ</option>
                                    <option value="MOSETÉN">MOSETÉN</option>
                                    <option value="MOVIMA">MOVIMA</option>
                                    <option value="MOXEÑO">MOXEÑO</option>
