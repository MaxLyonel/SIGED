{% extends 'layoutHerramienta.html.twig' %}

{% block stylesheets %}
    {{ parent() }}


    <link href="{{asset('css/bootstrap-fileupload.min.css')}}" rel="stylesheet">
    <link href="{{asset('css/bootstrap-fileupload.css')}}" rel="stylesheet">

{% endblock %}



{% block contentoption %}


    <!-- Start page header -->
    <div class="header-content">
        <h2><i class="fa fa-globe animated flip"></i> Bucar Información Consolidado <span></span></h2>
        <div class="breadcrumb-wrapper hidden-xs">
            <span class="label">Usted esta aqui:</span>
            <ol class="breadcrumb">
                <li>
                    <i class="fa fa-globe"></i>
                    <a href="#">
                    Bucar Información Consolidado
                    </a>
                    <i class="fa fa-angle-right"></i>
                </li>

                <li class="active">Index</li>
            </ol>
        </div><!-- /.breadcrumb-wrapper -->
    </div><!-- /.header-content -->
    <!--/ End page header -->


    <!-- /.row -->
  <div class="body-content animated fadeIn">
    <div class="row">
        <div class="col-xs-12 col-lg-12">
            <div class="panel panel-icon panel-primary">
                <div class="panel-heading">
                    <i class="fa fa-search"></i> Buscar SIE
                </div>
                <div class="panel-body">



                    <form name="form" method="post" action="{{ path('herramienta_infoconsolidation_find') }}" data-toggle="validator" role="form">
                        <div class="form-group">
                            <label for="form_codigoRude" class="col-sm-2 control-label">Sie</label>
                            <div class="col-sm-10">
                                <input type="text" id="form_codigoSie" name="form[codigoSie]" required="required" class="form-control" placeholder="Inserte SIE" pattern="[0-9]{6,8}" />
                                <div class="help-block with-errors"></div>
                            </div>

                        </div>

                        <div class="form-group">
                            <div class="col-sm-offset-2 col-sm-10">

                            </div>
                        </div>
                        <div class="form-group">
                            <div class="col-sm-offset-2 col-sm-10">

                                <button type="submit" id="form_buscar" name="form[buscar]" class="btn btn-primary">Buscar</button>
                            </div>
                        </div>
                    </form>
                </div>
            </div>

        </div>
    </div>
  </div>

{% endblock %}
