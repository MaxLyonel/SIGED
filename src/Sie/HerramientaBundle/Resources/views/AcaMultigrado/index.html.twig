                      //
                      // $("#responseId").append('<p><a class="btn btn-md btn-danger" href="{# path('ccalidad_list', {id: 5}) #}">Finalizar</a></p>');

                    },
                    statusCode: {
                        500: function () {

                        },
                        404: function () {

                        }
                    }
                });
        }
      };

