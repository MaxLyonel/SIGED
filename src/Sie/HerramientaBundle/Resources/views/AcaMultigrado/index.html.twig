                      // $("#studentOneId").css('display','none');
                      // $("#studentTwoId").css('display','none');
                      $("#responseIdMain").empty();
                      $("#responseIdMain").append(data);
                      $("#responseId").empty();

                    },
                    statusCode: {
                        500: function () {

