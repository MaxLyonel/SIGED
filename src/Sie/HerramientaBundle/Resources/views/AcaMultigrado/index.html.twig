                        404: function () {

                        }
                    }
                });
        }
      };
      var noMultigrado = function (){
        if(confirm('¿Esta seguro de realizar los cambios?\nPresione aceptar para confirmar.') ){
           var jsonDataMultigrado = $('#form_jsonDataMultigrado').val();
                $.ajax({
                    type: 'post',
                    url: Routing.generate('aca_multigrado_noMultigrado'),
