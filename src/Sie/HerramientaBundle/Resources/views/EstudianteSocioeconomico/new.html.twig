        var peticionCreate;
        function createSocioeconomico(){
            if (peticionCreate && peticionCreate.readyState != 4) {
                peticionCreate.abort();
            }

            // validar si tiene tutor y si la persona es nueva
            var idPersona = $('#t_idPersona').val();
            if(idPersona == 'nuevo' && $('#t_carnet').attr('required')){
                if($('#t_carnet').val() == ""){ $('#avisos').append('<div style="border-top:1px solid #949A9A;">6.1 Cédula de identidad</div>'); return false; }
                if($('#t_fechaNacimiento').val() == ""){ $('#avisos').append('<div style="border-top:1px solid #949A9A;">6.1 Fecha de Nacimiento</div>'); return false; }                
            }

            var t_carnet = $('#t_carnet').val();
            if(t_carnet.length > 0){
                var segipId = $('#t_segipId').val();
                var primeros = t_carnet.substring(0, 2);
                if((segipId == 0 || segipId == 12) && (primeros == '99' || primeros == '9-') && $('#t_carnet').attr('requried')){
                    alert('El padre o tutor asignado a la o el estudiante tiene un numero de carner incorrecto, debe realizar el cambio de los datos del padre o tutor haciendo clic en el boton "Cambiar padre o tutor" y registrando los datos correctos para poder realizar el registro del formulario RUDE.');
                    if(invalid.length <= 0){
                        cerrarAvisos();
                    }
                    return false;
                }
            }

            // validar si tiene madre y si la persona es nueva
            var idPersonam = $('#m_idPersona').val();
            if(idPersonam == 'nuevo' && $('#m_carnet').attr('required')){
                if($('#m_carnet').val() == ""){ $('#avisos').append('<div style="border-top:1px solid #949A9A;">6.1 Cédula de identidad</div>'); return false; }
                if($('#m_fechaNacimiento').val() == ""){ $('#avisos').append('<div style="border-top:1px solid #949A9A;">6.1 Fecha de Nacimiento</div>'); return false; }                
            }

            var m_carnet = $('#m_carnet').val();
            if(m_carnet.length > 0){
                var segipId = $('#m_segipId').val();
                var primeros = m_carnet.substring(0, 2);
                if((segipId == 0 || segipId == 12) && (primeros == '99' || primeros == '9-') && $('#m_carnet').attr('requried')){
                    alert('La madre asignada a la o el estudiante tiene un numero de carner incorrecto, debe realizar el cambio de los datos de la madre haciendo clic en el boton "Cambiar madre" y registrando los datos correctos para poder realizar el registro del formulario RUDE.');
                    if(invalid.length <= 0){
                        cerrarAvisos();
                    }
                    return false;
                }
            }

            // Validamos que al menos tenga un tutor o madre
            var tieneTutor = $('input:radio[name=t_tieneTutor]:checked').val();
            var tieneMadre = $('input:radio[name=m_tieneMadre]:checked').val();
            console.log(tieneTutor);
            console.log(tieneMadre);
            if(tieneTutor == 0 && tieneMadre == 0){
                alert('Debe registrar al menos un tutor o los datos de la madre!!!');
                return false;
            }

            var formData = new FormData(document.getElementById("formSocioeconomico"));

            peticionCreate = $.ajax({
                type: 'post',
                //data: $('#formSocioeconomico').serialize(),
                data: formData,
                dataType: 'html',
                cache: false,
                contentType: false,
                processData: false,
                url: Routing.generate('estudianteSocioeconomico_create'),
                beforeSend: function () {
