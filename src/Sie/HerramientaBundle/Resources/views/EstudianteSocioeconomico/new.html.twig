        function bloquearDiscapacidad(){
            var blocked = true;
            if($('#form_seccionvEstudianteDiscSensorial').val() == 1){ blocked = false; }
            if($('#form_seccionvEstudianteDiscMotriz').val() == 1){ blocked = false; }
            if($('#form_seccionvEstudianteDiscMental').val() == 1){ blocked = false; }
            if($('#form_seccionvEstudianteDiscOtro').val() != ""){ blocked = false; }

            if(blocked == true){
                $('#form_seccionvDiscTipo').removeAttr('required');
                $('#form_seccionvDiscTipo').val('');
                $('#form_seccionvDiscTipo').attr('disabled','disabled');
