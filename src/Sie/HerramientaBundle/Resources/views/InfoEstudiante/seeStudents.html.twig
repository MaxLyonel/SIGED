                                            ( app.session.get('ue_humanistica') ) or
                                            (app.session.get('ue_plena')  and app.session.get('ue_humanistica') ) or
                                            (app.session.get('ue_tecteg') and app.session.get('ue_humanistica') ) or
                                            (app.session.get('ue_humanistica_web') and app.session.get('ue_humanistica') ) or
                                            (app.session.get('ue_modular') and app.session.get('ue_humanistica') or false )
