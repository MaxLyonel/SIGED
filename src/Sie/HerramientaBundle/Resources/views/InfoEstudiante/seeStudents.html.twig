                        {% for data in objStudents %}
                          {# set items = {
                                          'estadomatriculaId': data.estadomatriculaId,
                                          'id': data.id,
                                          'carnetIdentidad': data.carnetIdentidad,
                                          'codigoRude': data.codigoRude,
