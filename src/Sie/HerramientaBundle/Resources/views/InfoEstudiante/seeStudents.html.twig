                                        {% set styleButton = 'success' %}
                                      {% else %}
                                        {% set styleButton = 'info' %}
                                      {% endif %}
                                    {# if  app.session.get('ue_humanistica') #}
                                    {% if
                                      ( app.session.get('ue_plena') ) or
                                      ( app.session.get('ue_modular') ) or
                                      ( app.session.get('ue_caldiff') ) or
                                      ( app.session.get('ue_humanistica_web') ) or
                                      ( app.session.get('ue_sol_regularizar') ) or
                                      ( app.session.get('ue_tecteg') )
                                    %}
                                      
                                      {% if data.estadomatriculaId == 4 or (data.estadomatriculaId in [5,11,55] and gestion == app.session.get('currentyear')) or app.session.get('ie_id') == '81981478'  or app.session.get('ie_id') == '81700043' %}
