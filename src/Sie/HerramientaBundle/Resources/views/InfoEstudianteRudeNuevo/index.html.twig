    function saveFormSocioeconomicos(){
        var data = $('#formSocioeconomicos').serialize();
        $.ajax({
            url: Routing.generate('info_estudiante_rude_nuevo_save_formSocioeconomicos'),
            type: 'post',
            data: data,
            beforeSend: function(){
                $('#cortina').css('display','block');
            },
            success: function(data){
                $('#cortina').css('display','none');
                $('#paso4').parent('li').removeClass('disabled');
                $('#paso4').attr('data-toggle','tab');
                $('#paso4').click();
                $('#tabPadre').click();
            },
            error: function(){
                $('#cortina').css('display','none');
            }
        });
    }

    function next(){
        var $active = $('.nav-tabs li.active');
        $active.next().removeClass('disabled');
        nextTab($active);
    }

    function prev(){
        var $active = $('.nav-tabs li.active');
        $active.next().removeClass('disabled');
        prevTab($active);
    }

    function nextTab(elem) {
        $(elem).next().find('a[data-toggle="tab"]').click();
    }
    function prevTab(elem) {
        $(elem).prev().find('a[data-toggle="tab"]').click();
    }

