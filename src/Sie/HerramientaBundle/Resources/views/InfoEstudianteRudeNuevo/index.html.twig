    function guardaJusticacion(rudeId){
        var obsExtTipo = document.getElementById('ObsExtTipo').value;
        var obsExtJust = document.getElementById('ObsExtJust').value;
        
        if (obsExtTipo.trim() === '' || obsExtJust.trim() === '') {
            Swal.fire({
                title: 'Error',
                text: 'Ambos campos son requeridos',
                type: 'error',
                confirmButtonText: 'Aceptar'
            });
            return; // No proceder con la acción de guardar
        }
        Swal.fire({
                        title: 'La Justificación a Registrar es temporal, y debe ser regularizada hasta antes de finalizar la gestión escolar',
                        text: "¿Esta seguro de seguro de guardar la justificación?",
                        type: 'question',
                        html: '¿Esta seguro de seguro de guardar la justificación?',
                        showCancelButton: true,
                        confirmButtonColor: '#3085d6',
                        cancelButtonColor: '#d33',
                        confirmButtonText: 'Si, proceder',
                        cancelButtonText: 'Cancelar'
                    }).then((result) => {
                        if (result.value) {
                            var imagen = '{{ asset("img/loading96.gif") }}';
                            $.ajax({
                                type: 'post',
                                url: "{{ path('info_estudiante_rude_nuevo_save_extObs') }}",
                                data:{
                                    rudeId: rudeId,
                                    obsExpTipo: obsExtTipo,
                                    obsExpJust: obsExtJust                                    
                                    },
                                beforeSend: function () {
                                },
                                success: function (data) {
                                    if (data.msg == 'ok'){
                                        ocultarJusticacion();
                                        $('#btnJustificarExt').removeClass('btn-danger').addClass('btn-warning btn-justificacion-existe');
                                        $('#btnJustificarExt').text('Existe Justificación');
                                        $('#btnJustificarExt').removeAttr('data-toggle');
                                    } else {
                                        ocultarJusticacion();
                                        alert('El estudiante ya cuenta con Justificación');
                                        $('#btnJustificarExt').removeClass('btn-danger').addClass('btn-warning btn-justificacion-existe');
                                        $('#btnJustificarExt').text('Existe Justificación');
                                        $('#btnJustificarExt').removeAttr('data-toggle');
                                    }
                                },
                                complete: function () {
                                    console.log("error");  
                                }
                            }); 
                        }
                    })

    }

