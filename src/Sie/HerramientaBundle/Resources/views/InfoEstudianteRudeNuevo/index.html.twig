    function validateFileSize(input) {
        var maxSize = 2 * 1024 * 1024; // 2 megabytes
        var files = input.files;

        if (files.length > 0 && files[0].size > maxSize) {
            alert('El tamaño del archivo no puede ser mayor a 2 MB.');
            input.value = ''; // Limpiar el campo de archivo
        }
    }

