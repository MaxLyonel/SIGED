{% extends 'layoutHerramienta.html.twig' %}

{% block contentoption %}

    <!-- Start page header -->
    <div class="header-content">
        <h2><i class="fa fa-table"></i> Formulario <span>equipamiento de laboratorio</span></h2>
        <div class="breadcrumb-wrapper hidden-xs">
            <span class="label">Usted esta aqui:</span>
            <ol class="breadcrumb">
                <li>
                    <i class="fa fa-home"></i>
                    <a href="dashboard.html">Equipamiento de laboratorio</a>
                    <i class="fa fa-angle-right"></i>
                </li>
                <li class="active">Index</li>
            </ol>
        </div><!-- /.breadcrumb-wrapper -->
    </div><!-- /.header-content -->
    <!--/ End page header -->
    <div class="body-content animated fadeIn">

        <div class="row">
            <div class="col-md-12">
                <section id="welcome" class="page-section">
                    <div class="panel panel-info">
                        <div class="panel-heading">
                            <h3 class="panel-title">Formulario</h3>
                        </div>
                        <div class="panel-body">
                            <div class="row">
                                {{form_start(form, { 'attr': {'data-toggle': 'validator','role': "form", 'class': 'form-horizontal'} } )}}
                                    <div class="form-body">
                                        <div class="form-group">
                                            <label class="col-lg-2 col-md-4 col-sm-6 col-xs-12 control-label">
                                                {{form_label(form.sie)}}                                    
                                            </label>
                                            <div class="col-lg-10 col-md-8 col-sm-6 col-xs-12">
                                                {{ form_widget(form.sie) }}
                                            </div>  
                                        </div>        
                                        <div class="form-group">       
                                            <div class="col-lg-12 col-md-12 col-sm-12 col-xs-12">
                                                {{ form_widget(form.search,{ 'attr':{'class':'btn btn-primary col-lg-12 col-md-12 col-sm-12 col-xs-12' } } ) }}
                                            </div>
                                        </div>
                                    </div>
                                {{form_end(form)}}
                            </div>
                        </div>
                    </div>

                </section>
            </div>
        </div>





        <div class='row'>
            <div class='col-md-12'>
                <div id='idmalla'></div>
            </div>
        </div>


    </div> 
{% endblock %}

