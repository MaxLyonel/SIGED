                                            {{ form_widget(form5.seccionvEstudianteEscentroSalud, {'attr':{'class':'jupper'} }) }}
                                        </div>
                                    </div>
                                    <div class="row form-group">
                                        <label class="col-md-6">5.2.2. ¿Cuantas veces fue la o el estudiante al centro de salud el año pasado?</label>
                                        <div class="col-md-6 col-sm-6">
                                            {{ form_widget(form5.seccionvCantCentrosaludTipo, {'attr':{'class':'form-control jupper'} }) }}
                                        </div>
                                    </div>
