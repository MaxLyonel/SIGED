                                                <div class="col-md-6">
                                                    Sensorial y de la comunicación
                                                </div>
                                                <div class="col-md-6">
                                                    {{ form_widget(form5.seccionvEstudianteEsdiscapacidadSensorialComunicacion, {'attr':{'class':'jupper'} }) }}
                                                </div>
                                            </div>
