    var actualizarInicioClases=function ()
    {
        var form=$('#form-actualizar-tipo').serialize();
        $.ajax({
                type: 'post',
                url: Routing.generate('siged_seleccionar_modalidad_tipo'),
                data: form,
                beforeSend: function () {
                    $(".div-mostrar-tipos").empty();
                    $(".div-mostrar-tipos").html("<div style='text-align:center'><img src='{{asset('img/loading.gif')}}'></div>");
                },
                success: function (data)
                {

                     setTimeout(function ()
                    {
                        $('#modalInicioClases').modal('toggle');
                    },1000);

                    var debido_a,text='';
                    debido_a=(data.riesgo=='Otros')?data.detallesInicio.observacion:data.riesgo;

                    if(data.riesgo==null)
                        text='La U.E. <b>SI</b> continuo con las actividades';
                    else
                        text='La U.E. <b>NO</b> continuo con las actividades, debido a: <b>'+debido_a+'</b>';
                    if(data.riesgo==null && data.detallesInicio==null)
                        text='No registro la información';

                    $(".div-mostrar-tipos").empty();
                    $(".div-mostrar-tipos").html(text);
                },
                error: function (data) {
                    $(".div-mostrar-tipos").empty();
                    $(".div-mostrar-tipos").append('<div class="alert alert-danger"><i class="glyphicon glyphicon-exclamation-sign"></i> No hay conexion con el servidor</div>');
