{% extends 'layoutHerramienta.html.twig' %}

{% block contentoption %} 


          <!-- Start page header -->
    <div class="header-content">
        <h2><i class="fa fa-list-alt"></i> Lista de Inscritos <span>Unidad Educativa</span></h2>
        <div class="breadcrumb-wrapper hidden-xs">
            <span class="label">Usted esta aqu√≠:</span>
            <ol class="breadcrumb">
                <li>
                    <i class="fa fa-home"></i>
                    <a href="dashboard.html">Lista de Inscritos / Buscar</a>
                    <i class="fa fa-angle-right"></i>
                </li>

            </ol>
        </div><!-- /.breadcrumb-wrapper -->
    </div><!-- /.header-content -->
    <!--/ End page header -->

    <!-- the new view -->

    <div class="body-content animated fadeIn">

        <div class="row">
            <div class="col-md-12">

                {% for flashMessage in app.session.flashbag.get('noticesi') %}
                    <div class="alert alert-danger alert-dismissible" role="alert">
                        <button type="button" class="close" data-dismiss="alert" aria-label="Close"><span aria-hidden="true">&times;</span></button>
                        <strong><i class='fa fa-linux'></i></strong> {{ flashMessage }}
                    </div>
                {% endfor %}
                {% for flashMessage in app.session.flashbag.get('successsi') %}
                    <div class="alert alert-success alert-dismissible" role="alert">
                        <button type="button" class="close" data-dismiss="alert" aria-label="Close"><span aria-hidden="true">&times;</span></button>
                        <strong><i class='fa fa-linux'></i></strong> {{ flashMessage }}
                    </div>
                {% endfor %}
            </div>

        </div>

          <div class="row">
        <div class="col-xs-12 col-lg-12">
            <div class="panel panel-icon panel-primary">
                <div class="panel-heading">
                    <i class="fa fa-search"></i>
                </div>
                <div class="panel-body">
                    {{form_start(form, { 'attr': {'data-toggle': 'validator','role': "form"} } )}}



                    <div class="form-group">
                        <label for="form_codigoRude" class="col-sm-2 control-label">
                            {{form_label(form.sie)}}
                        </label>
                        <div class="col-sm-10">
                            {{ form_widget(form.sie) }}
                            <div class="help-block with-errors"></div>
                        </div>

                    </div>

                    <div class="form-group">
                        <label for="form_codigoRude" class="col-sm-2 control-label">
                            {{form_label(form.gestion)}}
                        </label>
                        <div class="col-sm-10">
                            {{ form_widget(form.gestion) }}
                            <div class="help-block with-errors"></div>
                        </div>

                    </div>

                    <div class="form-group">
                        <div class="col-sm-offset-2 col-sm-10">

                            {{ form_widget(form.search,{ 'attr':{'class':'btn btn-primary' } } ) }}
                        </div>
                    </div>


                    {{form_end(form)}}
                </div>
            </div>

        </div>
    </div>
        
    </div>

    <!-- the new view end -->

    


    

    

    <!-- /.row -->

  

{% endblock %}
