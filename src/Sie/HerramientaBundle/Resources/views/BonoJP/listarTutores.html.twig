<script>
function buscar_validar_persona_ci_segip(){
	var ci=$("#ci").val();
	var complemento=$("#complemento").val();
	$.post('{{path('buscar_validar_persona_ci_segip')}}', {ci,complemento}, function(dato) {
        var valores = eval(dato);
		if (valores[0]===0) {
        	$('#contenido_fer').css('display', 'block');
			$('#ci1').val(ci);
            $("#ci1").removeAttr("disabled");

            $('#complemento1').val('');
            $("#complemento1").removeAttr("disabled");

            $('#form_idfecnac').val('');
            $("#form_idfecnac").removeAttr("disabled");

            $('#paterno').val('');
            $("#paterno").removeAttr("disabled");

            $('#materno').val('');
            $("#materno").removeAttr("disabled");

            $('#nombre').val('');
            $("#nombre").removeAttr("disabled");

            $('#idpersona').val('0');
		}else{
			$('#contenido_fer').css('display', 'block');
		    $('#ci1').val(valores[0]);
            $("#ci1").attr('disabled', 'disabled');

            $('#complemento1').val('');
            $("#complemento1").attr('disabled', 'disabled');

            $('#form_idfecnac').val(valores[4]);
            $("#form_idfecnac").attr('disabled', 'disabled');

            $('#paterno').val(valores[1]);
            $("#paterno").attr('disabled', 'disabled');

            $('#materno').val(valores[2]);
            $("#materno").attr('disabled', 'disabled');

            $('#nombre').val(valores[3]);
            $("#nombre").attr('disabled', 'disabled');

            $('#idpersona').val(valores[5]);
		}
	});
}
