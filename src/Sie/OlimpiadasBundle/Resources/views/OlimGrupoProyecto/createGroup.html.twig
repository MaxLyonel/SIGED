<div class="modal-dialog modal-lg">
{{form_start(form, {'attr':{'id':'createGroupFormId', 'class':'form-inline'} })}}
    <div class="modal-content">
        <div class="panel panel-info">
            <div class="panel-heading">
                <div class="pull-left">
                    <h3 class="panel-title">Registrar Grupo</h3>
                </div>
                <div class="pull-right">
                  
                </div>
                <div class="clearfix"></div>
            </div>
            <div class="panel-body">


              <div class="row">
                <div class="col-md-8 col-md-offset-3">
                  
                        <div class="form-group mx-sm-3 mb-2">
                          Nombre de equipo:
                          {{ form_widget(form.nombre, {'attr':{'class':'form-control', 'placeholder':'', 'pattern':'[0-9]{4,9}'} })}}

                        </div>
                        
                </div>
              </div>
                  
            <div class="row">
              <div class="col-md-12">
                  <div id="idanswerTutors">
                    
                  </div>
              </div>
            </div>

            </div>
            </div>
            <div class="modal-footer">
                <button type="button" class="btn btn-default" data-dismiss="modal">Cerrar</button>
                <button type="button" class="btn btn-info" data-dismiss="modal" onclick='createGroupSave()'>Guardar</button>
            </div>
    </div><!-- /.modal-content -->
    {{form_end(form)}}
</div><!-- /.modal-dialog -->



