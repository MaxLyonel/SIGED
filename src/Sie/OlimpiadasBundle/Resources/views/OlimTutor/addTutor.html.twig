<div class="modal-dialog modal-lg">
    <div class="modal-content">
        <div class="panel panel-info">
            <div class="panel-heading">
                <div class="pull-left">
                    <h3 class="panel-title">Registrar tutor</h3>
                </div>
                <div class="pull-right">
                    <button class="btn btn-sm" data-action="collapse" data-toggle="tooltip" data-placement="top" data-title="Collapse"><i class="fa fa-angle-up"></i></button>
                </div>
                <div class="clearfix"></div>
            </div>
            <div class="panel-body">


              <div class="row">
                <div class="col-md-8 col-md-offset-4">
                      {{form_start(form, {'attr':{'id':'addTutorFormId', 'class':'form-inline'} })}}
                        <div class="form-group mx-sm-3 mb-2">
                          {{ form_widget(form.carnet, {'attr':{'class':'form-control', 'placeholder':'Buscar por CI', 'pattern':'[0-9]{4,9}'} })}}
                        </div>
                        {{form_widget(form.buscar, {'attr':{'class':'btn btn-primary mb-2'} })}}
                     {{form_end(form)}}
                </div>
              </div>
                  
            <div class="row">
              <div class="col-md-12">
                  <div id="idanswerTutors">
                    
                  </div>
              </div>
            </div>

            </div>
            </div>
            <div class="modal-footer">
                <button type="button" class="btn btn-default" data-dismiss="modal">Cerrar</button>
                <!-- <button type="button" class="btn btn-theme" data-dismiss="modal" onclick='krlos()'>Guardar</button> -->
            </div>
    </div><!-- /.modal-content -->
</div><!-- /.modal-dialog -->



