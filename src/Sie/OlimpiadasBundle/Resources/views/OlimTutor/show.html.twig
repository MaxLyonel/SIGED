            <tr>
                <th>Usuarioregistroid</th>
                <td>{{ entity.usuarioRegistroId }}</td>
            </tr>
            <tr>
                <th>Usuariomodificacionid</th>
                <td>{{ entity.usuarioModificacionId }}</td>
            </tr>
        </tbody>
    </table>

        <ul class="record_actions">
    <li>
        <a href="{{ path('olimtutor') }}">
            Back to the list
        </a>
    </li>
    <li>
        <a href="{{ path('olimtutor_edit', { 'id': entity.id }) }}">
