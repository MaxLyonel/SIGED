{% extends 'layoutOlimpiadas.html.twig' %}

{% block contentoption %}

    <!-- Start page header -->
    <div class="header-content">
        <h2><i class="fa fa-pencil"></i> Inscripciones <span></span></h2>
        <div class="breadcrumb-wrapper hidden-xs">
            <span class="label">Usted esta aqui:</span>
            <ol class="breadcrumb">
                <li>
                    <i class="fa fa-pencil"></i>
                    <a href="{{path('infra_list_index')}}">
                       Inscripciones
                    </a>
                    <i class="fa fa-angle-right"></i>
                </li>


            </ol>
        </div><!-- /.breadcrumb-wrapper -->
    </div><!-- /.header-content -->
    <!--/ End page header -->

    <div class="body-content animated fadeIn">
        <div class="row">
            <div class="col-md-12">
                <div class="panel panel-info shadow">
                                <div class="panel-heading">
                                    <div class="pull-left">
                                        <h3 class="panel-title">Tutor</h3>
                                    </div>
                                    <div class="pull-right">
                                        <button class="btn btn-sm" data-action="collapse" data-toggle="tooltip" data-placement="top" data-title="Collapse" data-original-title="" title=""><i class="fa fa-angle-up"></i></button>
                                    </div>
                                    <div class="clearfix"></div>
                                </div><!-- /.panel-heading -->
                                <div class="panel-body no-padding">

                                    <!-- Start list message -->
                                    <div class="media-list">

                                        <a href="#" class="media">
                                            <div class="pull-left"><img src="{{asset('/webolimpiadas/img/avatar/50/24.png')}}" class="media-object img-circle" alt="..."></div>
                                            

                                            <div class="media-body">
                                                <span class="media-heading">
                                                    {{tutor.nombre}}
                                                    {{tutor.paterno}}
                                                    {{tutor.materno}}
                                                </span>
                                                <span class="media-text">
                                                    <b>Tel&eacute;fono 1:</b> {{tutor.telefono1}} </br>
                                                    <b>Tel&eacute;fono 2:</b> {{tutor.telefono2}} </br>
                                                    <b>email:</b> {{tutor.correo_electronico}}
                    
                                                </span>
                                                <!-- Start meta icon -->
                                                
                                                <!--/ End meta icon -->
                                            </div><!-- /.media-body -->

                                              

                                            

                                              
                                            

                                        </a><!-- /.media -->

                                    </div>
                                    <!--/ End list message -->

                                </div><!-- /.panel-body -->
                            </div>
            </div>
            
        </div>


    <div class="row" id="blog-single">

    <div class="col-lg-12 col-md-12 col-sm-12 col-xs-12">

        <h5 class="comment-count">Resultado de la b&uacute;squeda</h5>

        <ul class="media-list comment-list">

            <li class="media">
                <div class="media-left">
                    <a href="#">
                        <img class="media-object thumbnail" src="{{asset('/webolimpiadas/img/avatar/50/24.png')}}" alt="...">
                    </a>
                </div>
                <div class="media-body">
                    
  <div class="pull-right rounded">
                    
                  </div>
                    
                    <h4>
                      
                      
                      <b>C&oacute;digo Rude:</b> {{objStudentInscription.codigo_rude}} </br>
                      <b>Estudiante:</b> {{objStudentInscription.nombre}} 
                      {{objStudentInscription.paterno}} 
                      {{objStudentInscription.materno}} </br>
                      <b>CI-Complemento:</b> {{objStudentInscription.carnet_identidad}} {{objStudentInscription.complemento}} </br>

                    </h4>
                    <small class="text-muted">---</small>
                    <p>
                      <div class="row">
                        <div class="col-md-6"><b>C&oacute;digo Rude:</b> {{objStudentInscription.sie}} </div>
                        <div class="col-md-6"><b>Unidad Educativa:</b> {{objStudentInscription.institucioneducativa}} </div>
                      </div>
                      </br> 
                      <div class="row">
                            <div class="col-md-3"><b>Nivel</b> </div>
                            <div class="col-md-3"><b>Grado</b> </div>
                            <div class="col-md-3"><b>Paralelo</b> </div>
                            <div class="col-md-3"><b>Turno</b> </div>
                            
                        </div>
                        <div class="row">
                            <div class="col-md-3">{{objStudentInscription.nivel}} </div>
                            <div class="col-md-3">{{objStudentInscription.grado}} </div>
                            <div class="col-md-3">{{objStudentInscription.paralelo}} </div>
                            <div class="col-md-3">{{objStudentInscription.turno}} </div>
                            
                        </div>
                
                  </p>
                    

                
                </div>
            </li><!-- media -->


        </ul><!-- comment-list -->
        <br>                           
    </div>


    <div class="col-md-12">

      <table class="table table-bordered table-condensed records_list table-striped">
                            <thead>
                                <tr>
                                    {# <th>Id</th> #}
                                    <th>Categoria</th>
                                    <th>Modalidad</th>
                                   
                                    <th>Niveles</th>
                                    
                                </tr>
                            </thead>
                            <tbody>
                            {% for entity in entities %}
                                <tr>

                                    {# <td><a href="{{ path('olimreglasolimpiadastipo_show', { 'id': entity.id }) }}">{{ entity.id }}</a></td> #}
                                    <td><strong>{{ entity.entidad.categoria }}</strong></td>
                                    <td>
                                        {% if entity.entidad.modalidadParticipacionTipo.id == 1 %}
                                            <span class="badge badge-primary">
                                                {{ entity.entidad.modalidadParticipacionTipo.modalidad }}        
                                            </span>
                                        {% else %}
                                            <span class="badge badge-lilac">
                                                {{ entity.entidad.modalidadParticipacionTipo.modalidad }}        
                                            </span>
                                        {% endif %}
                                    </td>
                                    
                                    
                                    
                                    <td>

                                        {% if entity.entidad.modalidadParticipacionTipo.id == 1 %}    

                                            {% if entity.primaria|length > 0 %}
                                                <p><strong>Primaria</strong></p>
                                                {% for pri in entity.primaria %}
                                                    <table>
                                                        <tbody>
                                                            <tr>
                                                                <td><span class="badge badge-inverse badge-stroke"> {{ pri.gradoTipo.id }} </span></td>
                                                                <td>
                                                                    <form action="{{path('oliminscriptionsnivelsuperior_inscripcionsavesuperior')}}" method="post" style="display: inline-block" class="form-inline">
                                                                    <input type="hidden" id="form_nivel" name="form[nivel]" value="{{pri.nivelTipo.id}}">
                                                                    <input type="hidden" id="form_grado" name="form[grado]" value="{{pri.gradoTipo.id}}">

                                                                    <input type="hidden" id="form_category" name="form[category]" value="{{entity.entidad.id}}">
                                                                    <input type="hidden" id="form_jsonInfoToDoInscription" name="form[jsonInfoToDoInscription]" value="{{jsonInfoToDoInscription}}">

                                                                      <input type="text" id="form_fono" name="form[fono]" value=""   placeholder='telefono' class="form-control">
                                                                      <input type="text" id="form_email" name="form[email]" value="" placeholder='correo electronico' class="form-control">
                                                                  
                                                                  <select class="form-control" name="form[discapacidad]">
                                                                    {% for discapacidad in objDiscapacidad %}
                                                                    <option value="{{discapacidad.id}}">{{discapacidad.discapacidad}}</option>       
                                                                    {% endfor %}
                                                                    </select>




                                                                    <button type="submit" class="btn btn-danger btn-xs" onclick="return confirm('Se realizara la inscripcion del estudiante!. ¿Desea continuar?');" data-toggle="tooltip" data-placement="top" data-original-title="Inscribir">Inscribir</button>
                                                                    </form>

                                                                </td>
                                                                
                                                            </tr>
                                                        </tbody>
                                                    </table>
                                                  
                                                {% endfor %}
                                            {% endif %}
                                            {% if entity.secundaria|length > 0 %}
                                                <p style="padding-top: 10px;"><strong>Secundara</strong></p>
                                                {% for sec in entity.secundaria %}
                                                    <table>
                                                        
                                                        <tbody>
                                                            <tr>
                                                                <td><span class="badge badge-inverse badge-stroke"> {{ sec.gradoTipo.id }} </span></td>
                                                                <td>
                                                                     <form action="{{path('oliminscriptionsnivelsuperior_inscripcionsavesuperior')}}" method="post" style="display: inline-block" class="form-inline">
                                                                    <input type="hidden" id="form_nivel" name="form[nivel]" value="{{sec.nivelTipo.id}}">
                                                                    <input type="hidden" id="form_grado" name="form[grado]" value="{{sec.gradoTipo.id}}">

                                                                    <input type="hidden" id="form_category" name="form[category]" value="{{entity.entidad.id}}">

                                                                    <input type="hidden" id="form_jsonInfoToDoInscription" name="form[jsonInfoToDoInscription]" value="{{jsonInfoToDoInscription}}">

                                                                    <input type="text" id="form_fono" name="form[fono]" value=""   placeholder='telefono' class="form-control">
                                                                      <input type="text" id="form_email" name="form[email]" value="" placeholder='correo electronico' class="form-control">
                                                                     
                                                                     <select class="form-control" name="form[discapacidad]">
                                                                    {% for discapacidad in objDiscapacidad %}
                                                                    <option value="{{discapacidad.id}}">{{discapacidad.discapacidad}}</option>       
                                                                    {% endfor %}
                                                                    </select>


                                                                    <button type="submit" class="btn btn-danger btn-xs" onclick="return confirm('Se realizara la inscripcion del estudiante!. ¿Desea continuar?');" data-toggle="tooltip" data-placement="top" data-original-title="Inscribir">Inscribir</button>
                                                                    </form>

                                                                </td>
                                                            </tr>
                                                        </tbody>
                                                    </table>
                                                    
                                                    
                                                {% endfor %}
                                            {% endif %}
                                        {% endif %}
                                    </td>
                                    
                                </tr>
                            {% endfor %}
                            </tbody>
                        </table>
        
    </div>
</div>




    
    </div>

     <div class="modal fade bs-example-modal-table" tabindex="-1" role="dialog" aria-hidden="true" data-backdrop="static" id="modal">      
    </div>

{% endblock %}
{% block javascripts%}
    {{parent()}}
    <script type="text/javascript" charset="utf-8" >




    var specialInscription = function(){

      let formData = $('#specialInscriptionForm').serialize();

        $.ajax({
             type: 'post',
             url: Routing.generate('oliminscriptions_findStudentRule'),
             data: formData,
             beforeSend: function () {
                 $("#specialInscriptionId").empty();
                 $("#specialInscriptionId").empty();
                 $("#specialInscriptionId").append('<div class="text-center"><img src="{{asset('img/loading.gif')}}" class="" /></div>');
             },
             success: function (data) {
                 $("#specialInscriptionId").empty();
                 $("#specialInscriptionId").append(data);
                 //location.reload(true);
             },
             statusCode: {
                 500: function () {

                 },
                 404: function () {

                 }
             }
          });
    };




    </script>

    
    
{% endblock %}
