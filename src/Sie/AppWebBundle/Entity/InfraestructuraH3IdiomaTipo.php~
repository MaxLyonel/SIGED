<?php

namespace Sie\AppWebBundle\Entity;

use Doctrine\ORM\Mapping as ORM;

/**
 * InfraestructuraH3IdiomaTipo
 */
class InfraestructuraH3IdiomaTipo
{
    /**
     * @var integer
     */
    private $id;

    /**
     * @var string
     */
    private $infraestructuraIdioma;

    /**
     * @var string
     */
    private $obs;

    public function __toString(){
        return $this->infraestructuraIdioma;
    }


    /**
     * Get id
     *
     * @return integer 
     */
    public function getId()
    {
        return $this->id;
    }

    /**
     * Set infraestructuraIdioma
     *
     * @param string $infraestructuraIdioma
     * @return InfraestructuraH3IdiomaTipo
     */
    public function setInfraestructuraIdioma($infraestructuraIdioma)
    {
        $this->infraestructuraIdioma = $infraestructuraIdioma;
    
        return $this;
    }

    /**
     * Get infraestructuraIdioma
     *
     * @return string 
     */
    public function getInfraestructuraIdioma()
    {
        return $this->infraestructuraIdioma;
    }

    /**
     * Set obs
     *
     * @param string $obs
     * @return InfraestructuraH3IdiomaTipo
     */
    public function setObs($obs)
    {
        $this->obs = $obs;
    
        return $this;
    }

    /**
     * Get obs
     *
     * @return string 
     */
    public function getObs()
    {
        return $this->obs;
    }
}
