    }

    /**
     * Set estudianteAsignatura
     *
     * @param \Sie\AppWebBundle\Entity\EstudianteAsignatura $estudianteAsignatura
     * @return EstudianteNota
     */
    public function setEstudianteAsignatura(\Sie\AppWebBundle\Entity\EstudianteAsignatura $estudianteAsignatura = null)
