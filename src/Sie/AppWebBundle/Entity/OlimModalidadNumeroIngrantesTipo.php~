<?php

namespace Sie\AppWebBundle\Entity;

use Doctrine\ORM\Mapping as ORM;

/**
 * OlimModalidadNumeroIngrantesTipo
 */
class OlimModalidadNumeroIngrantesTipo
{
    /**
     * @var integer
     */
    private $id;

    /**
     * @var string
     */
    private $condicion;

    /**
     * @var integer
     */
    private $cantidadMiembros;


    /**
     * Get id
     *
     * @return integer 
     */
    public function getId()
    {
        return $this->id;
    }

    /**
     * Set condicion
     *
     * @param string $condicion
     * @return OlimModalidadNumeroIngrantesTipo
     */
    public function setCondicion($condicion)
    {
        $this->condicion = $condicion;
    
        return $this;
    }

    /**
     * Get condicion
     *
     * @return string 
     */
    public function getCondicion()
    {
        return $this->condicion;
    }

    /**
     * Set cantidadMiembros
     *
     * @param integer $cantidadMiembros
     * @return OlimModalidadNumeroIngrantesTipo
     */
    public function setCantidadMiembros($cantidadMiembros)
    {
        $this->cantidadMiembros = $cantidadMiembros;
    
        return $this;
    }

    /**
     * Get cantidadMiembros
     *
     * @return integer 
     */
    public function getCantidadMiembros()
    {
        return $this->cantidadMiembros;
    }
}
