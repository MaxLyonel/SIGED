<div class="row">
  <div class="col-md-12 text-center">
    <h3 class="text-primary">Programa Nacional de Post-alfabetización</h3>
  </div>
</div>
<div class="table-responsive">
  <div id="example-table_wrapper" class="dataTables_wrapper form-inline" role="grid">
    <table id="example-table" class="table table-striped table-bordered table-hover table-green dataTable" aria-describedby="example-table_info">
      <thead>
        <tr role="row">
          <th class="text-center">Gestión</th>
          <th class="text-center">Lugar</th>
          <th class="text-center">Periodo</th>
          <th class="text-center">Bloque</th>
          <th class="text-center">Parte</th>
          <th class="text-center">Fecha Inicio</th>
          <th class="text-center">Fecha Fin</th>
          <th class="text-center">Paralelo</th>
          <th class="text-center">Turno</th>
          <th class="text-center">Estado Matrícula</th>
          <th class="text-center">Acciones</th>
        </tr>
      </thead>

      <tbody role="alert" aria-live="polite" aria-relevant="all">
        {% for data in dataInscription %}
          <tr class = "success">
            <td>{{data.gestion_tipo_id_raep}}</td>
            <td>{{data.institucioneducativa_raep}}</td>
            <td>{{data.periodo_raep}}</td>
            <td>{{data.bloque_p }} </td>
            <td>{{data.parte_p}}</td>
            <td>{{data.fech_ini_p}}</td>
            <td>{{data.fech_fin_p}}</td>
            <td>{{data.paralelo_raep}} </td>
            <td>{{data.turno_raep}} </td>
            <td>{{data.estadomatricula_p}}</td>
            <td class="text-center">
              <form action="{{path('estudianteinformacion_result')}}" name="form" id="form" method="post">
                <input type="hidden" name="form[codigoRude]" id="form_codigoRude" value="{{datastudent.codigoRude}}">
                <input type="hidden" name="form[gestion]" id="form_gestion" value="{{data.gestion_tipo_id_raep}}">
                <button type="submit" class="btn-link"><i class="fa fa-plus-circle"></i> Ver más</button>
              </form>
            </td>
          </tr>
        {% endfor %}
      </tbody>
    </table>
  </div>
</div>