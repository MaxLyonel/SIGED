
<div class="col-md-12">
                            <!-- Start input grid -->
                            <div class="panel rounded shadow">
                                <div class="panel-heading">
                                    <div class="pull-left">
                                        <h3 class="panel-title">Datos Director(a)</h3>
                                    </div>
                                   
                                    <div class="clearfix"></div>
                                </div><!-- /.panel-heading -->
                                <div class="panel-body no-padding">

                                   	{{ form_start(form, {'attr':{'id':'idFormDirector'} }) }}
                                        <div class="form-body">

                                            <div class="form-group">
                                                <div class="row">
                                                    <div class="col-md-3">
                                                    	NOMBRE:
                                                        <input placeholder=".col-md-4" class="form-control" type="text" disabled="" value='{{dataDirector.nombre}}'>
                                                    </div>
                                                    <div class="col-md-3">
                                                    	PATERNO:
                                                        <input placeholder=".col-md-4" class="form-control" type="text" disabled="" value='{{dataDirector.paterno}}'>
                                                    </div>
                                                    <div class="col-md-3">
                                                    	MATERNO:
                                                        <input placeholder=".col-md-4" class="form-control" type="text" disabled="" value='{{dataDirector.materno}}'>
                                                    </div>
                                                    <div class="col-md-3">
                                                    	CI;
                                                        <input placeholder=".col-md-4" class="form-control" type="text" disabled="" value='{{dataDirector.carnet}}'>
                                                    </div>
                                                </div>
                                            </div>

                                            <div class="form-group">
                                                <div class="row">
                                                    <div class="col-md-4">
                                                    	TELEFONO 1:
                                                        {{ form_widget(form.telefono1, {'attr': {'class':'form-control', 'placeholder':'telefono 1'} }) }}
                                                    </div>
                                                    <div class="col-md-4">
                                                    	TELEFONO 2:
                                                        {{ form_widget(form.telefono2, {'attr':{'class': 'form-control', 'placeholder':'telefono 2'} } )}}
                                                    </div>
                                                    <div class="col-md-4">
                                                    	EMAIL:
                                                    	{{ form_widget(form.correoElectronico, {'attr':{'class':'form-control', 'placeholder':'email'} } )}}
                                                        
                                                    </div>
                                                </div>
                                            </div>

                                         
                                        </div><!-- /.form-body -->
                                        <div class="form-footer">
                                            <div class="pull-right">
                                                <!-- <button class="btn btn-danger mr-5">Cancel</button> -->
                                                {{form_widget(form.guardar, {'attr': {'class':'btn btn-primary'} })}}
                                            </div>
                                            <div class="clearfix"></div>
                                        </div><!-- /.form-footer -->
                                    {{form_end(form)}}

                                </div><!-- /.panel-body -->
                            </div><!-- /.panel -->
                            <!--/ End input grid -->
                        </div>
    {{ form(form) }}

