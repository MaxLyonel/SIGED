{% extends 'SieAppWebBundle:Default:sie.html.twig' %}

{% block contentoption%}


    <div id="page-wrapper">
        <div class="row">
            <div class="page-header">
                <h3 class="heading pull-left"><i class="fa fa-file-text animated flip"></i> Administraci&oacute;n de Usuarios <span class="sub-heading">{{app.session.get('fullname')}}</span></h3>
            </div>
            <!-- /.col-lg-12 -->
        </div>
        <!-- /.row -->
        <div class="separator bottom"></div>
        <div class="row">

            <div class="col-md-12">
                <!-- Website Stats  Widget -->
                <div class="panel" id="dashboard-tour-visits">
                    <!--h1 class="page-header">Resultado de la Busqueda</h1-->
                    <div class="container-fluid">
                        <div class="row">
                            <div class="col-lg-12">
                                <div class="panel panel-icon panel-success">
                                    <div class="panel-heading">
                                        <i class="fa fa-search"></i> Buscar Apoderados
                                    </div>
                                    <div class="panel-body">

                                        {{form_start(form)}}
                                        <div class="form-group">
                                            <label for="inputEmail3" class="col-sm-2 control-label">Rude</label>
                                            <div class="col-sm-10">
                                                {{ form_widget(form.codigoRude,{ 'attr':{'class': 'form-control','placeholder':'Inserte Rude'}  }) }}
                                            </div>
                                        </div>
                                        <div class="col-sm-offset-2 col-sm-10"></div>
                                        <div class="form-group">
                                            <label for="inputPassword3" class="col-sm-2 control-label">Gesti&oacute;n</label>
                                            <div class="col-sm-10">
                                                {{ form_widget(form.fechaNacimiento,{ 'attr':{'class': 'form-control','placeholder':'Inserte Rude'}  }) }}
                                            </div>
                                        </div>
                                        <div class="form-group">
                                            <div class="col-sm-offset-2 col-sm-10">

                                            </div>
                                        </div>
                                        <div class="form-group">
                                            <div class="col-sm-offset-2 col-sm-10">

                                                {{ form_widget(form.buscar,{ 'attr':{'class':'btn btn-default' } } ) }}
                                            </div>
                                        </div>
                                        {{form_end(form)}}
                                    </div>
                                </div>

                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>

    </div>         


{% endblock %}
