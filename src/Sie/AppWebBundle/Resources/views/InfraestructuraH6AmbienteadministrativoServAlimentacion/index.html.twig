<div class="table-responsive mb-20">
    
    <div class="btn-group tooltips" data-toggle="tooltip" data-container="body" title="" data-original-title="Adicionar">
        <button type="button" class="btn btn-inverse btn-danger btn-stroke " data-toggle="modal" data-target=".bs-example-modal-table" onclick="addnewservalimentacion('{{ambienteAdministrativoId}}')">
            Agregar Ambiente
        </button>
    </div>
    
    <table class="table">
        <thead>
            <tr>
                 <th>Ambiente</th>
                <th>Nro. Ambientes</th>
                <th>&Aacute;rea</th>
                <th>Estado Ambiente</th>
            </tr>
        </thead>
        <tbody>
        {% for entity in entities %}
            <tr>
                
                <td>{{ entity.n41AmbienteAlimentacionTipo }}</td>
                <td>{{ entity.n41NumeroAmbientes }}</td>
                <td>{{ entity.n41MetrosArea }}</td>
                <td>{{ entity.estadoTipo }}</td>
                
            </tr>
        {% endfor %}
        </tbody>
    </table>
</div>