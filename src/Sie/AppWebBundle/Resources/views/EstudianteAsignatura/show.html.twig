{% extends '::base.html.twig' %}

{% block body -%}
    <h1>EstudianteAsignatura</h1>

    <table class="record_properties">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ entity.id }}</td>
            </tr>
            <tr>
                <th>Ferchalastupdate</th>
                <td>{{ entity.ferchaLastUpdate|date('Y-m-d H:i:s') }}</td>
            </tr>
            <tr>
                <th>Version</th>
                <td>{{ entity.version }}</td>
            </tr>
            <tr>
                <th>Revisionid</th>
                <td>{{ entity.revisionId }}</td>
            </tr>
        </tbody>
    </table>

        <ul class="record_actions">
    <li>
        <a href="{{ path('nota_main') }}">
            Back to the list
        </a>
    </li>
    <li>
        <a href="{{ path('nota_main_edit', { 'id': entity.id }) }}">
            Edit
        </a>
    </li>
    <li>{{ form(delete_form) }}</li>
</ul>
{% endblock %}
