{% extends 'layoutPermanente.html.twig' %}

{% block stylesheets %}
    {{ parent() }}
    <link href="{{asset('assets/css/tablesorter.css')}}" rel="stylesheet" />
    <link href="{{asset('filefile/css/bootstrap-fileupload.min.css')}}" rel="stylesheet">
    <link href="{{asset('filefile/css/bootstrap-fileupload.css')}}" rel="stylesheet">
{% endblock %}

{% block contentoption%}
<div class='container'>
    <h1>OperativoSieTipo list</h1>

    <table class="records_list">
        <thead>
            <tr>
                <th>Id</th>
                <th>Operativo</th>
                <th>Descripcion</th>
                <th>Orden</th>
                <th>Fecharegistro</th>
                <th>Fechamodificacion</th>
                <th>Actions</th>
            </tr>
        </thead>
        <tbody>
        {% for entity in entities %}
            <tr>
                <td><a href="{{ path('operativesie_show', { 'id': entity.id }) }}">{{ entity.id }}</a></td>
                <td>{{ entity.operativo }}</td>
                <td>{{ entity.descripcion }}</td>
                <td>{{ entity.orden }}</td>
                <td>{% if entity.fechaRegistro %}{{ entity.fechaRegistro|date('Y-m-d H:i:s') }}{% endif %}</td>
                <td>{% if entity.fechaModificacion %}{{ entity.fechaModificacion|date('Y-m-d H:i:s') }}{% endif %}</td>
                <td>
                <ul>
                    <li>
                        <a href="{{ path('operativesie_show', { 'id': entity.id }) }}">show</a>
                    </li>
                    <li>
                        <a href="{{ path('operativesie_edit', { 'id': entity.id }) }}">edit</a>
                    </li>
                </ul>
                </td>
            </tr>
        {% endfor %}
        </tbody>
    </table>

        <ul>
        <li>
            <a href="{{ path('operativesie_new') }}">
                Create a new entry
            </a>
        </li>
    </ul>
</div>
{% endblock %}
{% block javascripts %}
{% endblock %}
