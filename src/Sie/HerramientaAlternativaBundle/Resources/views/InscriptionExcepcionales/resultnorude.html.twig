{% extends 'layoutHerramientaAlternativa.html.twig' %}

{% block contentoption%}
  <!-- Start page header -->
  <div class="header-content">
      <h2><i class="fa fa-table"></i> Inscripcion Excepcional<span>Resultado sin rudeal</span></h2>
      <div class="breadcrumb-wrapper hidden-xs">
          <span class="label">Usted esta aqui:</span>
          <ol class="breadcrumb">
                  <i class="fa fa-home"></i>
                  <a href="dashboard.html">Inscripcion Excepcional</a>
                  <i class="fa fa-angle-right"></i>
              </li>

              <li class="active">Resultado sin rudeal</li>
          </ol>
      </div><!-- /.breadcrumb-wrapper -->
  </div><!-- /.header-content -->
  <!--/ End page header -->

  <div class="body-content animated fadeIn">

  <!-- Start mail page -->
  <!--div class="container" style="margin-top:30px;"-->



  <div class="row">
  <div class="body-content animated fadeIn">
                      <div class='row'>
                        <div class='col-md-12'>
                        {% for flashMessage in app.session.flashbag.get('goodinscription') %}
                            <div class="alert alert-success alert-dismissible" >
                                <button type="button" class="close" data-dismiss="alert" aria-hidden="true">×</button>
                                <strong>Alerta!</strong> {{ flashMessage }}
                            </div>
                        {% endfor %}
                        </div>
                      </div>
                      <div class="row">
                          <div class="col-md-2 col-md-offset-10">
                              {{form_start(formninguno)}}
                              {{form_widget(formninguno.ninguno)}}
                              {{form_end(formninguno)}}
                          </div>
                      </div>

                      <div class="row">
                          <div class="col-md-12">
                              <!-- Start toggle switches -->
                              <div class="panel panel-success rounded shadow">
                                  <div class="panel-heading">
                                      <div class="pull-left">
                                          <h3 class="panel-title">   <i class="fa fa-search"></i> Inserte Rudeal</h3>
                                      </div>
                                      <div class="pull-right">
                                      </div>
                                      <div class="clearfix"></div>
                                  </div><!-- /.panel-heading -->
                                  <div class="panel-sub-heading">

                                  </div><!-- /.panel-sub-heading -->
                                  <div class="panel-body">
                                  <div class="table-responsive">
                                      <table id="myTable" class="table table-striped table-bordered table-hover table-green dataTable">
                                          <thead>
                                              <tr>
                                                  <th>Nombre</th>
                                                  <th>Paterno</th>
                                                  <th>Materno</th>
                                                  <th>Rudeal</th>
                                                  <th>CI</th>
                                                  <th>Acción</th>
                                                  <th>Acción</th>


                                              </tr>
                                          </thead>
                                          <tbody>
                                              {% for student in samestudents %}
                                                  <tr class="gradeU">
                                                      <td>{{student.nombre}}</td>
                                                      <td>{{student.paterno}}</td>
                                                      <td>{{student.materno}}</td>
                                                      <td class="center">{{student.codigoRude}}</td>
                                                      <td class="center">{{student.carnetIdentidad}}-{{student.complemento}}</td>
                                                      <td class="center">
                                                          <form id='form' name='form' action="{{path('inscription_extranjeros_result')}}" method="post">
                                                              <input type="hidden" name="id" value="{{student.id}}">
                                                              <input type="hidden" name="codigoRude" id="codigoRude" value="{{ student.codigoRude }}" />
                                                              <input type="hidden" name="pais" id="pais" value="{{newstudent.pais}}">
                                                              <input type="hidden" name="ueprocedencia" id=ueprocedencia"" value="{{newstudent.ueprocedencia}} "/>
                                                              <button class="btn btn-success btn-sm btn-block"> <i class="fa fa-pencil"></i> Inscribir</button>
                                                          </form>
                                                      </td>
                                                      <td class="center"><button class="btn btn-info btn-sm btn-block"><i class="fa fa-eye"></i> Historial</button> </td>
                                                  </tr>
                                              {% endfor %}
                                          </tbody>
                                      </table>
                                  </div>
                                  <!-- /.table-responsive -->


                                  </div><!-- /.panel-body -->
                              </div><!-- /.panel -->
                              <!-- End toggle switches -->

                          </div>
                  </div>

                  <div class="row">
                      <div class="col-md-12">
                      <div id='idinscriptionsStudent'></div>
                      </div>
                  </div>
          <!-- End of Website Stats Widget -->
      </div>
  </div>
  <!--/div-->
  <!--/ End mail page -->
  </div>
  <!-- Start body content -->
  <!--/ End body content -->
  <div class="modal fade bs-example-modal-table" tabindex="-1" role="dialog" aria-hidden="true" id="modal">

  </div><!-- /.modal -->
  <!--/ End body content -->
{% endblock %}
