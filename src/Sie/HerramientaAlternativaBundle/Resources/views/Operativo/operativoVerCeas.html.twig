<div class="modal-dialog">
    <div class="modal-content primary">
        <div class="modal-header">
            <button type="button" class="close" data-dismiss="modal">&times;</button>
            <h4 class="modal-title">Lista de CEAS del operativo</h4>
        </div>
        <div class="modal-body">
            <div class="row">
            <div class="table-responsive col-md-12">
                <table class="table table-bordered table-striped" id="datatable">
                    <thead>
                        <tr>
                            <th class="text-center" style="min-width:5px">No.</th>
                            <th>Cod. Cea</th>
                            <th>CEA</th>
                            <th class="text-center" style="min-width:5px">Sub Cea</th>
                        </tr>
                    </thead>
                    <tbody>
                        {% for ies in ies %}
                            <tr>
                                <td class="text-center">{{ loop.index }}</td>
                                <td>{{ ies.institucioneducativa.id }}</td>
                                <td >{{ ies.institucioneducativa.institucioneducativa }}</td>
                                <td class="text-center">{{ ies.sucursalTipo.id }}</td>
                            </tr>
                        {% endfor %}
                    </tbody>
                </table>
            </div>
            </div>
        </div>
    </div>
</div>