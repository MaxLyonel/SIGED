            <div class="user-info">
                <div class="image">
                    <img src="{{asset('webRue/assets/images/user.png')}}" width="48" height="48" alt="Usuario" />
                </div>
                <div class="info-container">
                    <div class="name" data-toggle="dropdown" aria-haspopup="true" aria-expanded="false">{{ app.session.get('name') }}</div>
                    <div class="email">{{ app.session.get('cuentauser') }}</div>
                    <div class="btn-group user-helper-dropdown">
                        <i class="material-icons" data-toggle="dropdown" aria-haspopup="true" aria-expanded="true">keyboard_arrow_down</i>
                        <ul class="dropdown-menu pull-right">
                            <li><a href="javascript:void(0);"><i class="material-icons">lock</i>Cambiar contraseña</a></li>
                            <li role="seperator" class="divider"></li>
                            <li><a href="{{path('logout')}}"><i class="material-icons">input</i>Cerrar sesión</a></li>
                        </ul>
                    </div>
                </div>
            </div>
            <!-- #User Info -->
            <!-- Menu -->
            <div class="menu">
                <ul class="list">
                    <li class="header">NAVEGACIÓN PRINCIPAL</li>
                    <li class="active">
                        <a href="{{path('principal_web')}}">
                            <i class="material-icons">home</i>
                            <span>Inicio</span>
                        </a>
                    </li>
                    <li>
                        <a href="{{path('certificado_rue')}}">
                            <i class="material-icons">content_copy</i>
                            <span>Certificado RUE</span>
                        </a>
                    </li>
                    <li>
                        <a href="{{path('le')}}">
                            <i class="material-icons">note_add</i>
                            <span>Registro de Local Educativo</span>
                        </a>
                    </li>
                    <li>
                        <a href="{{path('rue')}}">
                            <i class="material-icons">note_add</i>
                            <span>Registro de Instituciones Educativas</span>
                        </a>
                    </li>
                </ul>
            </div>
            <!-- #Menu -->
            <!-- Footer -->
            <div class="legal">
                <div class="copyright">
                    &copy; 2017 <a href="javascript:void(0);">Ministerio de Educación</a>.
                </div>
                <div class="version">
                    <b>Versión: </b> 2.0.0
                </div>
            </div>
            <!-- #Footer -->
        </aside>
        <!-- #END# Left Sidebar -->
    </section>

    <section class="content">
        <div class="container-fluid">
            {% block contentoption %}{% endblock %}
        </div>
    </section>

    {% block javascripts %}

        <!-- Jquery Core Js -->
        <script src="{{asset('webRue/assets/js/jquery.min.js')}}"></script>
        
        <!-- Bootstrap Core Js -->
